&#9;&#9;&#9;&#9;end
&#9;&#9;&#9;&#9;if server then
&#9;&#9;&#9;&#9;&#9;key_down_connect=Tool.KeyDown.OnServerEvent:connect(function(client, ...) if client == LocalPlayer then keyDownFunc(...) end end)
&#9;&#9;&#9;&#9;&#9;key_up_connect=Tool.KeyUp.OnServerEvent:connect(function(client, ...) if client == LocalPlayer then keyUpFunc(...) end end)
&#9;&#9;&#9;&#9;else
&#9;&#9;&#9;&#9;&#9;key_down_connect=Tool.KeyDownLocal.Event:connect(keyDownFunc)
&#9;&#9;&#9;&#9;&#9;key_up_connect=Tool.KeyUpLocal.Event:connect(keyUpFunc)
&#9;&#9;&#9;&#9;end
&#9;&#9;&#9;
&#9;&#9;&#9;&#9;SmokePart.Parent = handle
&#9;&#9;&#9;&#9;if not SmokePart:FindFirstChild(&apos;Weld&apos;) then
&#9;&#9;&#9;&#9;&#9;
&#9;&#9;&#9;&#9;&#9;local tweld=Instance.new(&apos;Weld&apos;)
