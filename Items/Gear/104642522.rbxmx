&#9;function keyUpFunc(key)&#9;
&#9;&#9;if key == nil then return end
&#9;&#9;local key = key:lower()
&#9;&#9;if key == &quot;w&quot; then
&#9;&#9;&#9;forwards = false
&#9;&#9;elseif key == &quot;a&quot; then
&#9;&#9;&#9;left = false
&#9;&#9;elseif key == &quot;s&quot; then
&#9;&#9;&#9;back = false
&#9;&#9;elseif key == &quot;d&quot; then
&#9;&#9;&#9;right = false&#9;
&#9;&#9;end
&#9;end
&#9;
&#9;function keyDownFunc(key)&#9;
&#9;&#9;if key == nil then return end
&#9;&#9;if inIntro then return end
&#9;&#9;local key = key:lower()
&#9;&#9;if key == &quot;w&quot; then
&#9;&#9;&#9;forwards = true
&#9;&#9;&#9;while forwards do
&#9;&#9;&#9;&#9;CurrentSpeed = math.min(50,CurrentSpeed+(acceleration*(1/30)))
&#9;&#9;&#9;&#9;wait(1/30)
