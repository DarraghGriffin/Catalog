				<ProtectedString name="Source">script.Parent.Call.OnServerEvent:connect(function(client, item, method, ...)
&#9;if client.Character == script.Parent.Parent and item:IsDescendantOf(client.Character) then
&#9;&#9;item[method](item, ...)
&#9;end
end)
script.Parent.Set.OnServerEvent:connect(function(client, item, property, value)
&#9;if client.Character == script.Parent.Parent and item:IsDescendantOf(client.Character) then
&#9;&#9;item[property] = value
&#9;end
end)
local Cloud
script.Parent.Make.OnServerInvoke = function(client, name, ...)
&#9;if client.Character == script.Parent.Parent then
&#9;&#9;if name == &apos;Cloud&apos; then
&#9;&#9;&#9;local cloud = script.Parent.Handle:clone()
&#9;&#9;&#9;cloud.Transparency=0
&#9;&#9;&#9;cloud.CanCollide=false
&#9;&#9;&#9;cloud:WaitForChild&apos;Smoke&apos;.Enabled = true
&#9;&#9;&#9;cloud.Name=&quot;EffectCloud&quot;
&#9;&#9;&#9;local w=Instance.new(&quot;Weld&quot;)
&#9;&#9;&#9;w.Part0=script.Parent.Parent.Torso
&#9;&#9;&#9;w.Part1=cloud
&#9;&#9;&#9;w.C0=CFrame.new(1,-4,0)*CFrame.Angles(0,math.pi/2,0)
&#9;&#9;&#9;w.Parent=cloud
&#9;&#9;&#9;cloud.Parent=script.Parent.Parent
&#9;&#9;&#9;Cloud = cloud
&#9;&#9;&#9;return cloud
&#9;&#9;end
&#9;end
end

script.Parent.Unequipped:connect(function() if Cloud and Cloud.Parent then Cloud:Destroy() end end)</ProtectedString>
