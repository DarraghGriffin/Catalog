	},
	StayUpright = GLib.Create'BodyGyro'{
		Name = 'DualDarkhearts_StayUpright',
		P = 1000000,
		maxTorque = Vector3.new(1, 0, 1)*1000000,
		cframe = CFrame.new()
	}
