	AttackAnim = WaitForChild(Tool, 'Lunge')
	if AttackAnim then
		AttackTrack = MyHumanoid:LoadAnimation(AttackAnim)
	end
	AttackTrack:Play()
	wait(0.4)
	if AttackTrack then
		AttackTrack:Stop()
	end
end
