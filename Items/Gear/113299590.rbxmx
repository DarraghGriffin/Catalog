				<ProtectedString name="Source">-- SolarCrane

local DebrisService = Game:GetService(&apos;Debris&apos;)
local PlayersService = Game:GetService(&apos;Players&apos;)
local Tool = script.Parent
local MyCharacter
local MyPlayer
local MyTorso
local Handle = Tool:FindFirstChild(&apos;Handle&apos;)


local ChairMesh = script:WaitForChild(&apos;ChairMesh&apos;)
local PianoMesh = script:WaitForChild(&apos;PianoMesh&apos;)
local Music = script:WaitForChild(&apos;Music&apos;)
local MeteorPianoScript = script:WaitForChild(&apos;MeteorPianoScript&apos;)

local ChairPrefab = Instance.new(&apos;Seat&apos;)
ChairPrefab.FormFactor = &apos;Custom&apos;
ChairPrefab.TopSurface = &apos;Smooth&apos;
ChairPrefab.BottomSurface = &apos;Smooth&apos;
ChairPrefab.Size = Vector3.new(2, 1.65, 2)
ChairMesh:Clone().Parent = ChairPrefab
local ChairGyro = Instance.new(&apos;BodyGyro&apos;)
ChairGyro.Parent = ChairPrefab


local PianoPrefab = Instance.new(&apos;Part&apos;)
PianoPrefab.FormFactor = &apos;Custom&apos;
PianoPrefab.TopSurface = &apos;Smooth&apos;
PianoPrefab.BottomSurface = &apos;Smooth&apos;
PianoPrefab.Size = Vector3.new(4.8, 2.85, 6)
Music:Clone().Parent = PianoPrefab
PianoMesh:Clone().Parent = PianoPrefab

local COOLDOWN_TIME = 35
local LAUNCH_DISTANCE = 200

local LastActivation = 0


local PossibleIncomingVectors = {}

-- Create table of possible incoming vectors
for x = -1, 1 do
&#9;for z = -1, 1 do
&#9;&#9;table.insert(PossibleIncomingVectors, Vector3.new(x, 1, z).unit)
&#9;end
end

function GetRandomFloat(minimum, maximum)
&#9;local unitRandom = math.random()
&#9;local interval = maximum - minimum
&#9;-- there is potential overflow here if the interval is larger than max float
&#9;if interval &lt;= 0 then
&#9;&#9;print(&quot;GetRandomFloat: The range beween minimum and maximum is less than zero.&quot;)
&#9;&#9;return nil
&#9;end
&#9;local randomNum = (unitRandom * interval) + minimum
&#9;return randomNum
end

function RandomPointInCircle(minRadius, maxRadius)
&#9;local outwardVector = Vector3.new(0,0,GetRandomFloat(minRadius, maxRadius))
&#9;local rotation = math.random() * math.pi * 2
&#9;return CFrame.Angles(0, rotation, 0) * outwardVector&#9;
end

function RayIgnoreCheck(hit)
&#9;if hit then
&#9;&#9;if hit.Transparency &gt;= 1 or string.lower(hit.Name) == &quot;water&quot; or
&#9;&#9;&#9;&#9;hit.Name == &quot;Effect&quot; or hit.Name == &quot;Rocket&quot; or hit.Name == &quot;Bullet&quot; or
&#9;&#9;&#9;&#9;hit.Name == &quot;Handle&quot; then
&#9;&#9;&#9;return true
&#9;&#9;end
&#9;end
&#9;return false
end

-- @preconditions: vec should be a unit vector, and 0 &lt; rayLength &lt;= 1000
function RayCast(startPos, vec, rayLength)
&#9;local hitObject, hitPos = game.Workspace:FindPartOnRay(Ray.new(startPos + (vec * .01), vec * rayLength), Handle)
&#9;if hitObject and hitPos then
&#9;&#9;local distance = rayLength - (hitPos - startPos).magnitude
&#9;&#9;if RayIgnoreCheck(hitObject) and distance &gt; 0 then
&#9;&#9;&#9;-- there is a chance here for potential infinite recursion
&#9;&#9;&#9;return RayCast(hitPos, vec, distance)
&#9;&#9;end
&#9;end
&#9;return hitObject, hitPos
end

local function FindTargetSpot()
&#9;return Workspace:FindFirstChild(&apos;TargetSpot&apos;)
end

local function FindOptimalLaunchVector(destination)
&#9;local bestDist = 0
&#9;local bestVec = Vector3.new(0, 1, 0)
&#9;local bestPos = destination + bestVec
&#9;for k, v in pairs(PossibleIncomingVectors) do
&#9;&#9;local hitObject, hitPos = RayCast(destination, v, LAUNCH_DISTANCE)
&#9;&#9;if (hitPos - destination).magnitude &gt; bestDist then
&#9;&#9;&#9;bestVec = v
&#9;&#9;&#9;bestDist = (hitPos - destination).magnitude
&#9;&#9;&#9;bestPos = hitPos
&#9;&#9;end
&#9;end
&#9;return bestVec, bestPos, bestDist
end



function onActivated()
&#9;if tick() - LastActivation &gt; COOLDOWN_TIME then
&#9;&#9;Tool.Enabled = false
&#9;&#9;LastActivation = tick()
&#9;&#9;Handle.Transparency = 1
&#9;&#9;delay(COOLDOWN_TIME - 1, function() Handle.Transparency = 0 Tool.Enabled = true end)
&#9;&#9;if MyTorso then
&#9;&#9;&#9;local offset = MyTorso.CFrame.lookVector * 3.5
&#9;&#9;&#9;local newChair = ChairPrefab:Clone()
&#9;&#9;&#9;local newPiano = PianoPrefab:Clone()
&#9;&#9;&#9;local fire = Instance.new(&apos;Fire&apos;)
&#9;&#9;&#9;fire.Enabled = false
&#9;&#9;&#9;fire.Heat = 14
&#9;&#9;&#9;fire.Parent = newPiano
&#9;&#9;&#9;newChair.CFrame = CFrame.new(MyTorso.CFrame.p, MyTorso.CFrame.p + offset)
&#9;&#9;&#9;newPiano.CFrame = CFrame.new(MyTorso.CFrame.p + offset, MyTorso.CFrame.p)
&#9;&#9;&#9;newChair.Parent = Workspace
&#9;&#9;&#9;newPiano.Parent = Workspace
&#9;&#9;&#9;DebrisService:AddItem(newChair, COOLDOWN_TIME - 1)
&#9;&#9;&#9;DebrisService:AddItem(newPiano, COOLDOWN_TIME - 1)
&#9;&#9;&#9;wait(2)
&#9;&#9;&#9;if newPiano:FindFirstChild(&apos;Music&apos;) then
&#9;&#9;&#9;&#9;newPiano.Music:Play()
&#9;&#9;&#9;end
&#9;&#9;&#9;for i = 1, 14 do
&#9;&#9;&#9;&#9;if newChair:FindFirstChild(&apos;SeatWeld&apos;) then
&#9;&#9;&#9;&#9;&#9;fire.Enabled = true
&#9;&#9;&#9;&#9;&#9;fire.Size = 3 + i
&#9;&#9;&#9;&#9;&#9;local pianoOffset = RandomPointInCircle(20, 100)
&#9;&#9;&#9;&#9;&#9;if i == 14 then -- hard-code last one to hit you
&#9;&#9;&#9;&#9;&#9;&#9;pianoOffset = Vector3.new()
&#9;&#9;&#9;&#9;&#9;end
&#9;&#9;&#9;&#9;&#9;local meteorPiano = PianoPrefab:Clone()
&#9;&#9;&#9;&#9;&#9;local meteorFire = Instance.new(&apos;Fire&apos;)
&#9;&#9;&#9;&#9;&#9;local newScript = MeteorPianoScript:Clone()
&#9;&#9;&#9;&#9;&#9;meteorFire.Heat = 20
&#9;&#9;&#9;&#9;&#9;meteorFire.Size = 18
&#9;&#9;&#9;&#9;&#9;meteorFire.Parent = meteorPiano
&#9;&#9;&#9;&#9;&#9;meteorPiano.Parent = Workspace
&#9;&#9;&#9;&#9;&#9;local targetPoint = Instance.new(&apos;Part&apos;)
&#9;&#9;&#9;&#9;&#9;targetPoint.Transparency = 1
&#9;&#9;&#9;&#9;&#9;targetPoint.CanCollide = false
&#9;&#9;&#9;&#9;&#9;targetPoint.Anchored = true
&#9;&#9;&#9;&#9;&#9;targetPoint.CFrame = CFrame.new(pianoOffset) + MyTorso.CFrame.p
&#9;&#9;&#9;&#9;&#9;DebrisService:AddItem(targetPoint, 30)
&#9;&#9;&#9;&#9;&#9;DebrisService:AddItem(meteorPiano, 30)
&#9;&#9;&#9;&#9;&#9;local pianoPropulsion = Instance.new(&apos;RocketPropulsion&apos;)
&#9;&#9;&#9;&#9;&#9;pianoPropulsion.Target = targetPoint
&#9;&#9;&#9;&#9;&#9;pianoPropulsion.MaxThrust = 100000
&#9;&#9;&#9;&#9;&#9;pianoPropulsion.MaxSpeed = 40
&#9;&#9;&#9;&#9;&#9;pianoPropulsion.Parent = meteorPiano
&#9;&#9;&#9;&#9;&#9;targetPoint.Parent = Workspace
&#9;&#9;&#9;&#9;&#9;local bestVec, bestPos, bestDist = FindOptimalLaunchVector(targetPoint.CFrame.p)
&#9;&#9;&#9;&#9;&#9;meteorPiano.CFrame = CFrame.new(bestPos)
&#9;&#9;&#9;&#9;&#9;pianoPropulsion:Fire()
&#9;&#9;&#9;&#9;&#9;newScript.Parent = meteorPiano
&#9;&#9;&#9;&#9;&#9;newScript.Disabled = false
&#9;&#9;&#9;&#9;else
&#9;&#9;&#9;&#9;&#9;newPiano.Music:Stop()
&#9;&#9;&#9;&#9;&#9;newPiano:Destroy()
&#9;&#9;&#9;&#9;&#9;newChair:Destroy()
&#9;&#9;&#9;&#9;&#9;return
&#9;&#9;&#9;&#9;end
&#9;&#9;&#9;&#9;wait(2)
&#9;&#9;&#9;end
&#9;&#9;&#9;wait(3)
&#9;&#9;&#9;if MyCharacter:FindFirstChild(&apos;Humanoid&apos;) then
&#9;&#9;&#9;&#9;MyCharacter.Humanoid.Jump = true
&#9;&#9;&#9;end
&#9;&#9;&#9;newChair:Destroy()
&#9;&#9;&#9;newPiano:Destroy()
&#9;&#9;end
&#9;end
end

function onEquipped()
&#9;MyCharacter = Tool.Parent
&#9;MyPlayer = MyCharacter and PlayersService:GetPlayerFromCharacter(MyCharacter)
&#9;MyTorso = MyCharacter and MyCharacter:FindFirstChild(&apos;Torso&apos;)
&#9;Tool.Grip = CFrame.new(0, 0, -1, -1, 0, 0, 0, 1, 0, 0, 0, -1)
&#9;Handle:WaitForChild(&apos;PianoMesh&apos;)
&#9;Handle.PianoMesh.Scale = Vector3.new(0.4,0.4,0.4)
end
&#9;
function onUnequipped()
&#9;
end

Tool.Equipped:connect(onEquipped)
Tool.Activated:connect(onActivated)
Tool.Unequipped:connect(onUnequipped)

