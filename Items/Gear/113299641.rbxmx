sp.Equipped:connect(function(mouse)
&#9;equipped=true
&#9;handle.Transparency=1
&#9;handle2.Transparency=1
&#9;if mouse~=nil then 
&#9;&#9;mouse.Icon=&quot;rbxasset://textures\\GunCursor.png&quot;
&#9;&#9;mouse.Button1Down:connect(function()
&#9;&#9;&#9;local chr=sp.Parent
&#9;&#9;&#9;if chr and check and mouse.Target~=nil and mouse.Target.Parent then
&#9;&#9;&#9;&#9;local t=chr:FindFirstChild(&quot;Torso&quot;)
&#9;&#9;&#9;&#9;local h=chr:FindFirstChild(&quot;Humanoid&quot;)
&#9;&#9;&#9;&#9;local tt=mouse.Target.Parent:FindFirstChild(&quot;Torso&quot;)
&#9;&#9;&#9;&#9;local th=mouse.Target.Parent:FindFirstChild(&quot;Humanoid&quot;)
&#9;&#9;&#9;&#9;if t and h and h.Health&gt;0 and equipped and check and th and th~=h and tt and (tt.Position-t.Position).magnitude&lt;=range and th.Health&gt;0 then
&#9;&#9;&#9;&#9;&#9;mouse.Icon=&quot;rbxasset://textures\\GunWaitCursor.png&quot;
&#9;&#9;&#9;&#9;&#9;check=false
&#9;&#9;&#9;&#9;&#9;sound:Play()
&#9;&#9;&#9;&#9;&#9;if flyanim then
&#9;&#9;&#9;&#9;&#9;&#9;flyanim:Stop()
&#9;&#9;&#9;&#9;&#9;end
&#9;&#9;&#9;&#9;&#9;flyanim=h:LoadAnimation(anim)
&#9;&#9;&#9;&#9;&#9;if flyanim then
&#9;&#9;&#9;&#9;&#9;&#9;flyanim:Play()
&#9;&#9;&#9;&#9;&#9;end
&#9;&#9;&#9;&#9;&#9;local bp=Instance.new(&quot;BodyPosition&quot;)
&#9;&#9;&#9;&#9;&#9;bp.position=tt.Position
&#9;&#9;&#9;&#9;&#9;bp.maxForce=Vector3.new(1,1,1)*(10^6)
&#9;&#9;&#9;&#9;&#9;debris:AddItem(bp,10)
&#9;&#9;&#9;&#9;&#9;bp.Parent=t
&#9;&#9;&#9;&#9;&#9;local bg=Instance.new(&quot;BodyGyro&quot;)
&#9;&#9;&#9;&#9;&#9;bg.cframe=CFrame.new(t.Position,tt.Position)
&#9;&#9;&#9;&#9;&#9;bg.maxTorque=Vector3.new(1,1,1)*(10^6)
&#9;&#9;&#9;&#9;&#9;debris:AddItem(bg,10)
&#9;&#9;&#9;&#9;&#9;bg.Parent=t
&#9;&#9;&#9;&#9;&#9;for i=1,1/rate do
&#9;&#9;&#9;&#9;&#9;&#9;wait(rate)
&#9;&#9;&#9;&#9;&#9;&#9;if bp and tt and t then
&#9;&#9;&#9;&#9;&#9;&#9;&#9;bp.position=tt.Position
&#9;&#9;&#9;&#9;&#9;&#9;&#9;bg.cframe=CFrame.new(t.Position,tt.Position)
&#9;&#9;&#9;&#9;&#9;&#9;end
&#9;&#9;&#9;&#9;&#9;end
&#9;&#9;&#9;&#9;&#9;if bp and bg and tt and th and h and t then
&#9;&#9;&#9;&#9;&#9;&#9;th.WalkSpeed=4
&#9;&#9;&#9;&#9;&#9;&#9;h.WalkSpeed=4
&#9;&#9;&#9;&#9;&#9;&#9;h.PlatformStand=true
&#9;&#9;&#9;&#9;&#9;&#9;bp:remove()
&#9;&#9;&#9;&#9;&#9;&#9;bg:remove()
&#9;&#9;&#9;&#9;&#9;&#9;local w=Instance.new(&quot;Motor&quot;)
&#9;&#9;&#9;&#9;&#9;&#9;w.Part0=tt
&#9;&#9;&#9;&#9;&#9;&#9;w.Part1=t
&#9;&#9;&#9;&#9;&#9;&#9;w.C1=CFrame.new(1.5,1,-1.3)*CFrame.Angles(1,0,0)
&#9;&#9;&#9;&#9;&#9;&#9;w.C0=CFrame.new(0,0,0)
&#9;&#9;&#9;&#9;&#9;&#9;debris:AddItem(w,5)
&#9;&#9;&#9;&#9;&#9;&#9;w.Parent=t
&#9;&#9;&#9;&#9;&#9;&#9;local damagerate=5/damage
&#9;&#9;&#9;&#9;&#9;&#9;for i=1,5/damagerate do
&#9;&#9;&#9;&#9;&#9;&#9;&#9;wait(damagerate)
&#9;&#9;&#9;&#9;&#9;&#9;&#9;local chomp=((i/(5/damagerate))*5)%1
&#9;&#9;&#9;&#9;&#9;&#9;&#9;if h and th and w and t and w.Parent==t then
&#9;&#9;&#9;&#9;&#9;&#9;&#9;&#9;if tempw1 and tempw2 then
&#9;&#9;&#9;&#9;&#9;&#9;&#9;&#9;&#9;tempw1.C0=CFrame.new(0,-.15+((1-math.sin(chomp*math.pi))*.2),-.6)
&#9;&#9;&#9;&#9;&#9;&#9;&#9;&#9;&#9;tempw2.C0=CFrame.new(0,-.35-((1-math.sin(chomp*math.pi))*.2),-.6)
&#9;&#9;&#9;&#9;&#9;&#9;&#9;&#9;end
&#9;&#9;&#9;&#9;&#9;&#9;&#9;&#9;h:TakeDamage(-1)
&#9;&#9;&#9;&#9;&#9;&#9;&#9;&#9;th:TakeDamage(1)
&#9;&#9;&#9;&#9;&#9;&#9;&#9;end
&#9;&#9;&#9;&#9;&#9;&#9;end
&#9;&#9;&#9;&#9;&#9;&#9;if tempw1 and tempw2 then
&#9;&#9;&#9;&#9;&#9;&#9;&#9;tempw1.C0=CFrame.new(0,-.15,-.6)
&#9;&#9;&#9;&#9;&#9;&#9;&#9;tempw2.C0=CFrame.new(0,-.35,-.6)
&#9;&#9;&#9;&#9;&#9;&#9;end
&#9;&#9;&#9;&#9;&#9;end
&#9;&#9;&#9;&#9;&#9;if h and th then
&#9;&#9;&#9;&#9;&#9;&#9;th.WalkSpeed=16
&#9;&#9;&#9;&#9;&#9;&#9;h.WalkSpeed=16
&#9;&#9;&#9;&#9;&#9;&#9;h.PlatformStand=false
&#9;&#9;&#9;&#9;&#9;end
&#9;&#9;&#9;&#9;&#9;if flyanim then
&#9;&#9;&#9;&#9;&#9;&#9;flyanim:Stop()
&#9;&#9;&#9;&#9;&#9;end
&#9;&#9;&#9;&#9;&#9;wait(3)
&#9;&#9;&#9;&#9;&#9;if mouse~=nil then
&#9;&#9;&#9;&#9;&#9;&#9;mouse.Icon=&quot;rbxasset://textures\\GunCursor.png&quot;
&#9;&#9;&#9;&#9;&#9;end
&#9;&#9;&#9;&#9;&#9;check=true
&#9;&#9;&#9;&#9;end
&#9;&#9;&#9;end
&#9;&#9;end)
&#9;&#9;delay(0,function()
&#9;&#9;&#9;local chr=sp.Parent
&#9;&#9;&#9;if chr then
&#9;&#9;&#9;&#9;head=nil
&#9;&#9;&#9;&#9;face=nil
&#9;&#9;&#9;&#9;local head=chr:FindFirstChild(&quot;Head&quot;)
&#9;&#9;&#9;&#9;if head~=nil then
&#9;&#9;&#9;&#9;&#9;face=head:FindFirstChild(&quot;face&quot;)
&#9;&#9;&#9;&#9;&#9;if face~=nil and face.Texture~=theface then
&#9;&#9;&#9;&#9;&#9;&#9;storedface=face.Texture
&#9;&#9;&#9;&#9;&#9;&#9;face.Texture=theface
&#9;&#9;&#9;&#9;&#9;end
&#9;&#9;&#9;&#9;&#9;if fakefangstop then
&#9;&#9;&#9;&#9;&#9;&#9;fakefangstop:remove()
&#9;&#9;&#9;&#9;&#9;end
&#9;&#9;&#9;&#9;&#9;if fakefangsbottom then
&#9;&#9;&#9;&#9;&#9;&#9;fakefangsbottom:remove()
&#9;&#9;&#9;&#9;&#9;end
&#9;&#9;&#9;&#9;&#9;
&#9;&#9;&#9;&#9;&#9;fakefangstop=handle:clone()
&#9;&#9;&#9;&#9;&#9;fakefangstop.Transparency=0
&#9;&#9;&#9;&#9;&#9;fakefangstop.CanCollide=false
&#9;&#9;&#9;&#9;&#9;tempw1=Instance.new(&quot;Motor&quot;)
&#9;&#9;&#9;&#9;&#9;tempw1.Part0=head
&#9;&#9;&#9;&#9;&#9;tempw1.Part1=fakefangstop
&#9;&#9;&#9;&#9;&#9;tempw1.C0=CFrame.new(0,-.15,-.6)
&#9;&#9;&#9;&#9;&#9;tempw1.Parent=fakefangstop
&#9;&#9;&#9;&#9;&#9;debris:AddItem(fakefangstop,10*60)
&#9;&#9;&#9;&#9;&#9;fakefangstop.Parent=head
&#9;&#9;&#9;&#9;&#9;
&#9;&#9;&#9;&#9;&#9;fakefangsbottom=handle2:clone()
&#9;&#9;&#9;&#9;&#9;fakefangsbottom.Transparency=0
&#9;&#9;&#9;&#9;&#9;fakefangsbottom.CanCollide=false
&#9;&#9;&#9;&#9;&#9;tempw2=Instance.new(&quot;Motor&quot;)
&#9;&#9;&#9;&#9;&#9;tempw2.Part0=head
&#9;&#9;&#9;&#9;&#9;tempw2.Part1=fakefangsbottom
&#9;&#9;&#9;&#9;&#9;tempw2.C0=CFrame.new(0,-.35,-.6)
&#9;&#9;&#9;&#9;&#9;tempw2.Parent=fakefangsbottom
&#9;&#9;&#9;&#9;&#9;debris:AddItem(fakefangsbottom,10*60)
&#9;&#9;&#9;&#9;&#9;fakefangsbottom.Parent=head
&#9;&#9;&#9;&#9;end
&#9;&#9;&#9;end
&#9;&#9;end)
&#9;end
&#9;if weld then
&#9;&#9;weld.Parent=sp
&#9;end 
end)

sp.Unequipped:connect(function()
&#9;handle.Transparency=0
&#9;handle2.Transparency=0
&#9;equipped=false
&#9;delay(0,function()
&#9;&#9;if face and face~=nil then
&#9;&#9;&#9;face.Texture=storedface
&#9;&#9;end
&#9;end)
&#9;if face and face~=nil then
&#9;&#9;face.Texture=storedface
&#9;end
&#9;if weld then
&#9;&#9;weld.Parent=sp
&#9;end
&#9;if flyanim then
&#9;&#9;flyanim:Stop()
&#9;end
&#9;if fakefangstop then
&#9;&#9;fakefangstop:remove()
&#9;end
&#9;if fakefangsbottom then
&#9;&#9;fakefangsbottom:remove()
&#9;end
end)

handle.Transparency=0
handle2.Transparency=0

