function RandomByPercent(Table)
	local Max = 0
	for i, v in pairs(Table) do
		Max = (Max + v.Percentage)
	end
	local Chosen = (math.random() * Max)
	local Last
	local Total = 0
	for i, v in pairs(Table) do
		Total = (Total + v.Percentage)
		Last = v
		if Total >= Chosen then
			break
		end
	end
	return Last, Chosen
end

