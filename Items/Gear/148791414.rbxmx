end

script.ChildAdded:connect(function(nchild)
	local humanoid = FindAttachedHumanoid(script)
	local ani = Instance.new('Animation')
	ani.AnimationId= nchild.Value
	local aniTrack=humanoid:LoadAnimation(ani)
	aniTrack:Play()
	Delay(0, function()
		nchild:Destroy()
	end)
end)]]></ProtectedString>
