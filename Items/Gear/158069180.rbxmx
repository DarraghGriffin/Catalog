	-- Variables
	State = 'Idle', -- Idle, Slashing, Lunging
	SlashingStartedAt = 0,
	AttackTicket = 0,
	Tracks = {},
	LeftSword = nil,
	SpinDebounce = false,
	Spins = 0,
	SpinTicket = 0,
	PoisonedCharacters = {},
	SpinForce = GLib.Create'BodyAngularVelocity'{
