				<string name="Name">Air</string>
				<ProtectedString name="Source">local equalizingForce = 236 / 1.2 -- amount of force required to levitate a mass


function windforce(part)
&#9;if (part.Parent:FindFirstChild(&quot;ForceField&quot;) ~= nil) then return end
&#9;if (part.Anchored == true) then return end
&#9;if part:FindFirstChild(&quot;WindEffect&quot;) == nil then

&#9;&#9;local dir = script.Parent.Velocity.unit
&#9;&#9;local force = Instance.new(&quot;BodyVelocity&quot;)
&#9;&#9;force.Name = &quot;WindEffect&quot;
&#9;&#9;local m = part:GetMass()
&#9;&#9;-- assume surface area proportion to mass
&#9;&#9;m = math.sqrt(m)
&#9;&#9;if (m &gt; 64) then m = 64 end
&#9;&#9;force.maxForce = Vector3.new(1,1,1) * equalizingForce * m * 10
&#9;&#9;force.P = 125
&#9;&#9;force.velocity =  (dir * 75) + Vector3.new(0, 30,0)
&#9;&#9;force.Parent = part
&#9;&#9;game.Debris:AddItem(force, .5)
&#9;end
end


function OnTouch(part)
&#9;if (part.Parent == nil) then return end
&#9;--if (part.Parent:FindFirstChild(&quot;Humanoid&quot;) ~= nil) then
&#9;--&#9;local t = part.Parent:FindFirstChild(&quot;Torso&quot;)
&#9;--&#9;if (t ~= nil) then
&#9;&#9;&#9;windforce(part)
&#9;--&#9;end
&#9;--end
&#9;script.Parent:Remove()
end


script.Parent.Touched:connect(OnTouch)


wait(2)
script.Parent:Remove()</ProtectedString>
