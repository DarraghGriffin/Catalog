IceTexture = &quot;http://www.roblox.com/asset?id=186936495&quot;

Animations = {
&#9;HoldUp = {Animation = Tool:WaitForChild(&quot;HoldUp&quot;), FadeTime = nil, Weight = nil, Speed = 6, Duration = 1},
