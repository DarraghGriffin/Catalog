					<Content name="LinkedSource"><null></null></Content>
					<string name="Name">Animate</string>
					<ProtectedString name="Source"><![CDATA[--Made by Luckymaxer

Figure = script.Parent

Humanoid = Figure:WaitForChild("Humanoid")

Players = game:GetService("Players")
Debris = game:GetService("Debris")
RunService = game:GetService("RunService")

Head = Figure:WaitForChild("Head")
Torso = Figure:WaitForChild("Torso")

Neck = Torso:FindFirstChild("Neck")
LeftShoulder = Torso:FindFirstChild("Left Shoulder")
RightShoulder = Torso:FindFirstChild("Right Shoulder")
LeftHip = Torso:FindFirstChild("Left Hip")
RightHip = Torso:FindFirstChild("Right Hip")

Creator = Figure:WaitForChild("Creator")
Offset = Figure:WaitForChild("Offset")

Amplitude = 0.2
Frequency = 2

NeckAmplitude = 0.2
NeckFrequency = 2

function RayCast(Position, Direction, MaxDistance, IgnoreList)
	return game:GetService("Workspace"):FindPartOnRayWithIgnoreList(Ray.new(Position, Direction.unit * (MaxDistance or 999.999)), IgnoreList) 
end

function SecureJump()
	if Humanoid.Jump then
		return
	end
	local TargetPoint = Humanoid.TargetPoint
	local Blockage, BlockagePos = RayCast((Torso.CFrame + CFrame.new(Torso.Position, Vector3.new(TargetPoint.X, Torso.Position.Y, TargetPoint.Z)).lookVector * (Torso.Size.Z / 2)).p, Torso.CFrame.lookVector, (Torso.Size.Z * 2.5), {Figure, (((Creator and Creator.Value and Creator.Value:IsA("Player") and Creator.Value.Character) and Creator.Value.Character) or nil)})
	local Jumpable = false
	if Blockage then
		Jumpable = true
		if Blockage:IsA("Terrain") then
			local CellPos = Blockage:WorldToCellPreferSolid((BlockagePos - Vector3.new(0, 2, 0)))
			local CellMaterial, CellShape, CellOrientation = Blockage:GetCell(CellPos.X, CellPos.Y, CellPos.Z)
			if CellMaterial == Enum.CellMaterial.Water then
				Jumpable = false
			end
		elseif Blockage.Parent:FindFirstChild("Humanoid") then
			Jumpable = false
		end
	end
	if Jumpable then
		Humanoid.Jump = true
	end
end

Humanoid.Died:connect(function()
	Debris:AddItem(Figure, 5)
	if Humanoid and Humanoid.Parent then
		Humanoid:Destroy()
	end
	for i, v in pairs(Figure:GetChildren()) do
		if v:IsA("BasePart") then
			v.Name = "Part"
			v.CanCollide = false
		end
	end
end)

while Humanoid.Health > 0 do
	_, Time = wait(0.05)
	SecureJump()
	local CreatorValue = Creator.Value
	if CreatorValue then
		local CreatorPlayer = ((CreatorValue:IsA("Player") and CreatorValue) or Players:GetPlayerFromCharacter(Creator.Value))
		if CreatorPlayer then
			local CreatorCharacter = CreatorPlayer.Character
			if CreatorCharacter and CreatorCharacter.Parent then
				local CreatorHumanoid = CreatorCharacter:FindFirstChild("Humanoid")
				local CreatorTorso = CreatorCharacter:FindFirstChild("Torso")
				if CreatorHumanoid and CreatorHumanoid.Health > 0 and CreatorTorso then
					Humanoid:MoveTo((CreatorTorso.CFrame * CFrame.new(Offset.Value.X, Offset.Value.Y, Offset.Value.Z)).p)
				end
			end
		end
	end

	if Torso.Velocity.magnitude > 5 then
		Amplitude = 0.5
		Frequency = (Humanoid.WalkSpeed * 0.5)
		Neck.DesiredAngle = 0
		NeckAmplitude = 0
		NeckFrequency = 0
	else
		Amplitude = 0.2
		Frequency = 2
		NeckAmplitude = 0.2
		NeckFrequency = 2
	end
	
	NeckDesiredAngle = (NeckAmplitude * math.sin(Time * NeckFrequency))
	
	ArmDesiredAngle = (Amplitude * math.sin(Time * Frequency))
	LegDesiredAngle = (Amplitude * math.sin((Time * Frequency) + 0.5))

	Neck.DesiredAngle = NeckDesiredAngle
	LeftShoulder.DesiredAngle = ArmDesiredAngle
	RightShoulder.DesiredAngle = ArmDesiredAngle
	LeftHip.DesiredAngle = LegDesiredAngle
	RightHip.DesiredAngle = LegDesiredAngle

end]]></ProtectedString>
