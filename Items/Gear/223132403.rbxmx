				<ProtectedString name="Source"><![CDATA[--Made by Luckymaxer

Tool = script.Parent
Handle = Tool:WaitForChild("Handle")

Players = game:GetService("Players")
Debris = game:GetService("Debris")

Remover = script:WaitForChild("Remover")

BasePart = Instance.new("Part")
BasePart.Shape = Enum.PartType.Block
BasePart.Material = Enum.Material.Plastic
BasePart.TopSurface = Enum.SurfaceType.Smooth
BasePart.BottomSurface = Enum.SurfaceType.Smooth
BasePart.FormFactor = Enum.FormFactor.Custom
BasePart.Size = Vector3.new(0.2, 0.2, 0.2)
BasePart.CanCollide = true
BasePart.Locked = true
BasePart.Anchored = false

Grips = {
	Equipped = CFrame.new(0, -0.75, 0, 0.99999994, 0, 0, 0, 1, 0, 0, 0, 1),
	Display = CFrame.new(3, 3.1500001, -2, 0.89442718, -0, 0.44721359, -0, 1, -0, -0.44721359, -0, 0.89442718),
}

Animations = {
}

Sounds = {
}

Controls = {
	Forward = false,
	Backward = false,
	Left = false,
	Right = false
}

Speed = 10

Gravity = 196.20

MovementTime = (1 / 60)
ReloadTime = 5

HotWheelsTwinductionCar = require(Tool:WaitForChild("VehicleCreator"))

ServerControl = (Tool:FindFirstChild("ServerControl") or Instance.new("RemoteFunction"))
ServerControl.Name = "ServerControl"
ServerControl.Parent = Tool

ClientControl = (Tool:FindFirstChild("ClientControl") or Instance.new("RemoteFunction"))
ClientControl.Name = "ClientControl"
ClientControl.Parent = Tool

Handle.Transparency = 0
Tool.Grip = Grips.Equipped
Tool.Enabled = true

ServerControl.OnServerInvoke = (function(player, Mode, Value)
	if player == Player then
		if Mode == "MouseClick" and Value.Down then
			Activated()
		elseif Mode == "KeyPress" then
			local Key = Value.Key
			local ByteKey = string.byte(Key)
			local Down = Value.Down
			if Key == "w" or ByteKey == 17 then
				Controls.Forward = Down
			elseif Key == "s" or ByteKey == 18 then
				Controls.Backward = Down
			elseif Key == "a" or ByteKey == 20 then
				Controls.Left = Down
			elseif Key == "d" or ByteKey == 19 then
				Controls.Right = Down
			end
		end
	end
end)

function InvokeClient(Mode, Value)
	local ClientReturn = nil
	pcall(function()
		ClientReturn = ClientControl:InvokeClient(Player, Mode, Value)
	end)
	return ClientReturn
end

function GetTotalMass(Parent)
	local Mass = 0
	local function GetMass(Parent)
		for i, v in pairs(Parent:GetChildren()) do
			if v:IsA("BasePart") then
				Mass = (Mass + v:GetMass())
			end
		end
	end
	GetMass(Parent)
	return Mass
end

function TransformModel(Objects, Center, NewCFrame, Recurse)
	Objects = ((type(Objects) ~= "table" and {Objects}) or Objects)
	for i, v in pairs(Objects) do
		if v:IsA("BasePart") then
			v.CFrame = NewCFrame:toWorldSpace(Center:toObjectSpace(v.CFrame))
		end
		if Recurse then
			TransformModel(v:GetChildren(), Center, NewCFrame, true)
		end
	end
end

function IsFlipped(Object)
	local Vector1 = Object.CFrame:vectorToWorldSpace(Vector3.FromNormalId(Enum.NormalId.Top))
	local Vector2 = Vector3.new(0, 1, 0)
	local Angle = math.acos(Vector1:Dot(Vector2))
	return (Angle > 1.5)
end

function Activated()
	if not Tool.Enabled or not CheckIfAlive() or (PlayerHotWheelsTwinductionCar and PlayerHotWheelsTwinductionCar.Parent) then
		return
	end
	Tool.Enabled = false
	Humanoid.WalkSpeed = 0
	Handle.Transparency = 1
	local CarName = (Player.Name .."'s" .. HotWheelsTwinductionCar.Name)
	local Car = game:GetService("Workspace"):FindFirstChild(CarName)
	if Car then
		Car:Destroy()
	end
	PlayerHotWheelsTwinductionCar = HotWheelsTwinductionCar:Clone()
	PlayerHotWheelsTwinductionCar.Name = CarName
	local RemoverClone = Remover:Clone()
	local CreatorValue = Instance.new("ObjectValue")
	CreatorValue.Name = "Creator"
	CreatorValue.Value = Player
	CreatorValue.Parent = RemoverClone
	local ToolValue = Instance.new("ObjectValue")
	ToolValue.Name = "Tool"
	ToolValue.Value = Tool
	ToolValue.Parent = RemoverClone
	RemoverClone.Disabled = false
	RemoverClone.Parent = PlayerHotWheelsTwinductionCar
	CarBody = PlayerHotWheelsTwinductionCar:FindFirstChild("Body")
	FrontLeftMotor = CarBody:FindFirstChild("FrontLeftMotor")
	FrontRightMotor = CarBody:FindFirstChild("FrontRightMotor")
	BackLeftMotor = CarBody:FindFirstChild("BackLeftMotor")
	BackRightMotor = CarBody:FindFirstChild("BackRightMotor")
	if CarBody then
		CarBody.Elasticity = 0.5
		CarBody.Friction = 0.3
		BodyGyro = Instance.new("BodyGyro")
		BodyGyro.maxTorque = Vector3.new(math.huge, math.huge, math.huge)
		TransformModel(PlayerHotWheelsTwinductionCar, PlayerHotWheelsTwinductionCar:GetModelCFrame(), Handle.CFrame, true)
		BodyGyro.cframe = CarBody.CFrame
		BodyGyro.Parent = CarBody
		Debris:AddItem(BodyGyro, 0.5)
		BodyVelocity = Instance.new("BodyVelocity")
		BodyVelocity.P = 1250
		BodyVelocity.velocity = Vector3.new(0, 0, 0)
		BodyVelocity.maxForce = Vector3.new(0, 0, 0)
		BodyVelocity.Parent = CarBody
		BodyAngularVelocity = Instance.new("BodyAngularVelocity")
		BodyAngularVelocity.P = 1250
		BodyAngularVelocity.angularvelocity = Vector3.new(0, 0, 0)
		BodyAngularVelocity.maxTorque = Vector3.new(0, 0, 0)
		BodyAngularVelocity.Parent = CarBody
		PlayerHotWheelsTwinductionCar.Parent = game:GetService("Workspace")
		Car = game:GetService("Workspace"):FindFirstChild(CarName)
		if Car then
			InvokeClient("SetCamera", {Subject = Car, Type = Enum.CameraType.Track})
		end
		Spawn(function()
			local Mass = (GetTotalMass(PlayerHotWheelsTwinductionCar) * Gravity)
			while PlayerHotWheelsTwinductionCar and PlayerHotWheelsTwinductionCar.Parent do
				if not IsFlipped(CarBody) then
					if Controls.Left or Controls.Right then
						BodyAngularVelocity.maxTorque = Vector3.new(0, Mass, 0)
						BodyAngularVelocity.angularvelocity = Vector3.new(0, ((((Controls.Left and not Controls.Right) and 2) or ((Controls.Right and not Controls.Left) and -2)) or 0), 0)
					else
						BodyAngularVelocity.maxTorque = Vector3.new(0, 0, 0)
						BodyAngularVelocity.angularvelocity = Vector3.new(0, 0, 0)
					end
					if Controls.Forward or Controls.Backward then
						BodyVelocity.maxForce = Vector3.new(Mass, (Mass / 2), Mass)
						BodyVelocity.velocity = ((((Controls.Backward and not Controls.Forward) and (CarBody.CFrame.lookVector * ((Speed / 2) * -1))) or ((Controls.Forward and not Controls.Backward) and (CarBody.CFrame.lookVector * Speed))) or Vector3.new(0, 0, 0))
					else
						BodyVelocity.maxForce = Vector3.new(0, 0, 0)
						BodyVelocity.velocity = Vector3.new(0, 0, 0)
					end
				else
					BodyAngularVelocity.maxTorque = Vector3.new(0, 0, 0)
					BodyAngularVelocity.angularvelocity = Vector3.new(0, 0, 0)
					BodyVelocity.maxForce = Vector3.new(0, 0, 0)
					BodyVelocity.velocity = Vector3.new(0, 0, 0)
				end
				local WheelSpeed = (25 * ((((Controls.Backward and not Controls.Forward) and 1) or ((Controls.Forward and not Controls.Backward) and -1)) or 0))
				local DesiredAngle = (99999999999 * (-WheelSpeed / math.abs(WheelSpeed)))
				local MaxVelocity = (WheelSpeed / 250)
				for i, v in pairs({FrontLeftMotor, FrontRightMotor, BackLeftMotor, BackRightMotor}) do
					if v and v.Parent then
						v.DesiredAngle = DesiredAngle
						v.MaxVelocity = MaxVelocity
					end
				end
				wait(MovementTime)
			end
		end)
	end
	wait(ReloadTime)
	Tool.Enabled = true
end

function CheckIfAlive()
	return (Player and Player.Parent and Character and Character.Parent and Humanoid and Humanoid.Parent and Humanoid.Health > 0)
end

function Equipped()
	Character = Tool.Parent
	Player = Players:GetPlayerFromCharacter(Character)
	Humanoid = Character:FindFirstChild("Humanoid")
	if not CheckIfAlive() then
		return
	end
end

function Unequipped()
	if PlayerHotWheelsTwinductionCar and PlayerHotWheelsTwinductionCar.Parent then
		PlayerHotWheelsTwinductionCar:Destroy()
	end
	if Humanoid and Humanoid.Parent then
		InvokeClient("SetCamera", {Subject = Humanoid, Type = Enum.CameraType.Custom})
		Humanoid.WalkSpeed = 16
	end
	for i, v in pairs(Controls) do
		Controls[i] = false
	end
	for i, v in pairs(Animations) do
		InvokeClient("StopAnimation", v)
	end
	Handle.Transparency = 0
end

Tool.Equipped:connect(Equipped)
Tool.Unequipped:connect(Unequipped)]]></ProtectedString>
			</Properties>
			<Item class="Script" referent="RBX5C5C4C64A5B443A49DA82E17723D066C">
				<Properties>
					<bool name="Disabled">true</bool>
					<Content name="LinkedSource"><null></null></Content>
					<string name="Name">Remover</string>
					<ProtectedString name="Source"><![CDATA[--Made by Luckymaxer

Model = script.Parent

Debris = game:GetService("Debris")

FixCamera = script:WaitForChild("FixCamera")

Creator = script:FindFirstChild("Creator")
Tool = script:FindFirstChild("Tool")

function DestroyModel()
	if Creator and Creator.Parent then
		local FixCameraClone = FixCamera:Clone()
		FixCameraClone.Disabled = false
		if Character and Character.Parent then
			FixCameraClone.Parent = Character
		else
			local Backpack = Creator:FindFirstChild("Backpack")
			if Backpack then
				FixCameraClone.Parent = Backpack
			end
		end
	end
	Debris:AddItem(Model, 2)
end

if not Creator or not Creator.Value or not Creator.Value:IsA("Player") or not Creator.Value.Parent or not Tool or not Tool.Value or not Tool.Value.Parent then
	DestroyModel()
	return
end

Creator = Creator.Value
Tool = Tool.Value

Character = Creator.Character
if not Character then
	DestroyModel()
	return
end

Creator.Changed:connect(function(Property)
	if Property == "Parent" and not Creator.Parent then
		DestroyModel()
	end
end)

Character.Changed:connect(function(Property)
	if Property == "Parent" and not Character.Parent then
		DestroyModel()
	end
end)

Tool.Changed:connect(function(Property)
	if Property == "Parent" then
		DestroyModel()
	end
end)]]></ProtectedString>
				</Properties>
				<Item class="LocalScript" referent="RBX066EA51B44334CE08EDBF49ECBE93621">
