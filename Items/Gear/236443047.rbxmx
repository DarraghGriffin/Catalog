				<string name="Name">Server</string>
				<ProtectedString name="Source"><![CDATA[local Tool = script.Parent
local Remote = Tool:WaitForChild("Remote")
local Handle = Tool:WaitForChild("Handle")

local FriendlyFire = false

--returns the wielding player of this tool
function getPlayer()
	local char = Tool.Parent
	return game:GetService("Players"):GetPlayerFromCharacter(char)
end

--helpfully checks a table for a specific value
function contains(t, v)
	for _, val in pairs(t) do
		if val == v then
			return true
		end
	end
	return false
end

--tags a human for the ROBLOX KO system
function tagHuman(human)
	local tag = Instance.new("ObjectValue")
	tag.Value = getPlayer()
	tag.Name = "creator"
	tag.Parent = human
	game:GetService("Debris"):AddItem(tag)
end

--used by checkTeams
function sameTeam(otherHuman)
	local player = getPlayer()
	local otherPlayer = game:GetService("Players"):GetPlayerFromCharacter(otherHuman.Parent)
	if player and otherPlayer then
		if player == otherPlayer then
			return true
		end
		if otherPlayer.Neutral then
			return false
		end
		return player.TeamColor == otherPlayer.TeamColor
	end
	return false
end

--use this to determine if you want this human to be harmed or not, returns boolean
function checkTeams(otherHuman)
	return not (sameTeam(otherHuman) and not FriendlyFire)
end

function toggleHidden(model, hidden)
	local trans = 0
	if hidden then
		trans = 1
	end
	
	for _, obj in pairs(model:GetChildren()) do
		if obj:IsA("BasePart") then
			obj.Transparency = trans
		end
	end
end

function unanchor(model)
	for _, obj in pairs(model:GetChildren()) do
		if obj:IsA("BasePart") then
			obj.Anchored = false
		end
	end
end

function onEquip()
	local sound = Handle.Flop:Clone()
	Handle:Destroy()
	
	local root = Tool.Parent.HumanoidRootPart
	
	local model = game:GetService("InsertService"):LoadAsset(236140654):GetChildren()[1]
	unanchor(model)
	
	local connection
	connection = model.HumanoidRootPart.Touched:connect(function(part)
		if part:IsGrounded() then
			local w = Instance.new("Weld")
			w.Part0 = part
			w.Part1 = model.HumanoidRootPart
			w.C0 = part.CFrame:toObjectSpace(model.HumanoidRootPart.CFrame)
			w.Parent = w.Part0
			connection:disconnect()
		end
	end)
	
	sound.Parent = model.HumanoidRootPart
	
	toggleHidden(model, true)
	
	model.Parent = workspace
	model:SetPrimaryPartCFrame(root.CFrame * CFrame.new(0, -3, -2))
	model.AnimationController:LoadAnimation(Tool.CarpetFlop):Play()
	wait(0.2)
	
	toggleHidden(model, false)
	wait(0.6)
	
	sound:Play()
	game:GetService("Debris"):AddItem(model, 60)
	game:GetService("Debris"):AddItem(Tool, 0)
end

Tool.Equipped:connect(onEquip)]]></ProtectedString>
