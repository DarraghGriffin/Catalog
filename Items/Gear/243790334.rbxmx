				<string name="Name">Server</string>
				<ProtectedString name="Source"><![CDATA[local Tool = script.Parent
local Remote = Tool:WaitForChild("Remote")
local Handle = Tool:WaitForChild("Handle")

local FriendlyFire = false

local ArmMesh, HoldWeld, StabilityGyro
local CanGrab = true
local Gyros = {}
local Player

--returns the wielding player of this tool
function getPlayer()
	local char = Tool.Parent
	return game:GetService("Players"):GetPlayerFromCharacter(char)
end

--helpfully checks a table for a specific value
function contains(t, v)
	for _, val in pairs(t) do
		if val == v then
			return true
		end
	end
	return false
end

--tags a human for the ROBLOX KO system
function tagHuman(human)
	local tag = Instance.new("ObjectValue")
	tag.Value = getPlayer()
	tag.Name = "creator"
	tag.Parent = human
	game:GetService("Debris"):AddItem(tag)
end

--used by checkTeams
function sameTeam(otherHuman)
	local player = getPlayer()
	local otherPlayer = game:GetService("Players"):GetPlayerFromCharacter(otherHuman.Parent)
	if player and otherPlayer then
		if player == otherPlayer then
			return true
		end
		if otherPlayer.Neutral then
			return false
		end
		return player.TeamColor == otherPlayer.TeamColor
	end
	return false
end

--use this to determine if you want this human to be harmed or not, returns boolean
function checkTeams(otherHuman)
	return not (sameTeam(otherHuman) and not FriendlyFire)
end

function onTouched(part)
	if part:IsDescendantOf(Tool.Parent) then return end
	if not CanGrab then return end
	
	if part.Parent and part.Parent:FindFirstChild("Humanoid") then
		local human = part.Parent.Humanoid
		local root = part.Parent:FindFirstChild("HumanoidRootPart")
		if root and not root.Anchored then
			human.Sit = true
			
			if HoldWeld then
				HoldWeld:Destroy()
			end
			
			local w = Instance.new("Weld")
			w.Part0 = Handle
			w.Part1 = root
			w.C0 = CFrame.new(0, -1.1,  0.7) * CFrame.Angles(0, math.pi/2, 0) * CFrame.Angles(-math.pi/2, 0, 0)
			w.Parent = w.Part0
			HoldWeld = w
			
			Remote:FireClient(getPlayer(), "PlayAnimation", "Hold")
			
			local myRoot = Tool.Parent:FindFirstChild("HumanoidRootPart")
			if myRoot then
				local StabilityGyro = Instance.new("BodyGyro")
				StabilityGyro.maxTorque = Vector3.new(1e9, 0, 1e9)
				StabilityGyro.cframe = myRoot.CFrame
				StabilityGyro.Parent = myRoot
				table.insert(Gyros, StabilityGyro)
			end 
		end
	end
end

function onEquip()
	--put in our right arm
	local char = Tool.Parent
	local arm = Tool.ArmMesh:Clone()
	arm.Parent = char:FindFirstChild("Right Arm")
	ArmMesh = arm
	
	Player = getPlayer()
end

function onUnequip()
	if ArmMesh then
		ArmMesh:Destroy()
		ArmMesh = nil
	end
	if HoldWeld then
		HoldWeld:Destroy()
		HoldWeld = nil
	end
	if StabilityGyro then
		StabilityGyro:Destroy()
		StabilityGyro = nil
		
		for _, gyro in pairs(Gyros) do
			gyro:Destroy()
		end
		Gyros = {}
	end
	
	Remote:FireClient(Player, "StopAnimation", "Hold")
end

function onLeftDown()
	if HoldWeld then
		Remote:FireClient(getPlayer(), "StopAnimation", "Hold")
		
		local thisRoot = Tool.Parent:FindFirstChild("HumanoidRootPart")
		local thatRoot = HoldWeld.Part1
		if thisRoot and thatRoot then
			local human = thatRoot.Parent:FindFirstChild("Humanoid")
			if human then
				human.Sit = true
				
				HoldWeld:Destroy()
				HoldWeld = nil
				if StabilityGyro then
					StabilityGyro:Destroy()
					StabilityGyro = nil
				end
				
				local bv = Instance.new("BodyVelocity")
				bv.maxForce = Vector3.new(1e9, 1e9, 1e9)
				bv.velocity = thisRoot.CFrame.lookVector * 64
				bv.Parent = thatRoot
				game:GetService("Debris"):AddItem(bv, 0.2)
				
				CanGrab = false
				delay(0.5, function()
					CanGrab = true
				end)
			end
		end
	end
end

function onRemote(player, func, ...)
	if player ~= getPlayer() then return end
	
	if func == "LeftDown" then
		onLeftDown(...)
	end
end

Tool.Equipped:connect(onEquip)
Tool.Unequipped:connect(onUnequip)
Handle.Touched:connect(onTouched)
Remote.OnServerEvent:connect(onRemote)]]></ProtectedString>
