		local RightWingMesh = Instance.new("SpecialMesh")
		RightWingMesh.MeshType = Enum.MeshType.FileMesh
		RightWingMesh.MeshId = (BaseUrl .. "243182674")
		RightWingMesh.TextureId = (BaseUrl .. "243182721")
