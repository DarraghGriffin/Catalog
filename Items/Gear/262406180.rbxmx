				<ProtectedString name="Source"><![CDATA[--Made by Luckymaxer

Tool = script.Parent
Handle = Tool:WaitForChild("Handle")

Players = game:GetService("Players")
Debris = game:GetService("Debris")

SparkleScript = script:WaitForChild("SparkleScript")

RbxUtility = LoadLibrary("RbxUtility")
Create = RbxUtility.Create

BaseUrl = "http://www.roblox.com/asset/?id="

BasePart = Create("Part"){
	Shape = Enum.PartType.Block,
	Material = Enum.Material.Plastic,
	TopSurface = Enum.SurfaceType.Smooth,
	BottomSurface = Enum.SurfaceType.Smooth,
	FormFactor = Enum.FormFactor.Custom,
	Size = Vector3.new(0.2, 0.2, 0.2),
	CanCollide = true,
	Locked = true,
	Anchored = false,
}

Rate = (1 / 60)

LastSparkle = 0

ToolEquipped = false

Handle.Transparency = 0
Tool.Enabled = true

function CheckTableForString(Table, String)
	for i, v in pairs(Table) do
		if string.find(string.lower(String), string.lower(v)) then
			return true
		end
	end
	return false
end

function CheckIntangible(Hit)
	local ProjectileNames = {"Water", "Arrow", "Projectile", "Effect", "Rail", "Laser", "Bullet"}
	if Hit and Hit.Parent then
		if ((not Hit.CanCollide or CheckTableForString(ProjectileNames, Hit.Name)) and not Hit.Parent:FindFirstChild("Humanoid")) then
			return true
		end
	end
	return false
end

function CastRay(StartPos, Vec, Length, Ignore, DelayIfHit)
	local Ignore = ((type(Ignore) == "table" and Ignore) or {Ignore})
	local RayHit, RayPos, RayNormal = game:GetService("Workspace"):FindPartOnRayWithIgnoreList(Ray.new(StartPos, Vec * Length), Ignore)
	if RayHit and CheckIntangible(RayHit) then
		if DelayIfHit then
			wait()
		end
		RayHit, RayPos, RayNormal = CastRay((RayPos + (Vec * 0.01)), Vec, (Length - ((StartPos - RayPos).magnitude)), Ignore, DelayIfHit)
	end
	return RayHit, RayPos, RayNormal
end

function Activated()
	if not Tool.Enabled then
		return
	end
end

function EquipShoes(CreateNew)
	if not CheckIfAlive() then
		return
	end
	for i, v in pairs(Tool:GetChildren()) do
		if v:IsA("BasePart") and v ~= Handle then
			v:Destroy()
		end
	end
	if not CreateNew then
		return
	end
	local Shoe = BasePart:Clone()
	Shoe.CanCollide = false
	Shoe.Size = Vector3.new(1, 1.75, 1)
	local Mesh = Create("SpecialMesh"){
		MeshType = Enum.MeshType.FileMesh,
		Scale = Vector3.new(1.075, 1, 1),
		TextureId = (BaseUrl .. "257655677"),
		Parent = Shoe,
	}
	local ParticlePart = BasePart:Clone()
	ParticlePart.Name = "ParticlePart"
	ParticlePart.Transparency = 1
	ParticlePart.CanCollide = false
	for i = 1, 3 do
		local Sparkles = Create("Sparkles"){
			Name = "Sparkles",
			Enabled = false,
			Parent = ParticlePart,
		}
	end
	local ParticleWeld = Create("Weld"){
		Part0 = Shoe,
		Part1 = ParticlePart,
		C0 = CFrame.new(0, 0, ((Shoe.Size.Z / 1) + (ParticlePart.Size.Z / 2))),
		Parent = ParticlePart,
	}
	ParticlePart.Parent = Shoe
	LeftLeg = Character:FindFirstChild("Left Leg")
	RightLeg = Character:FindFirstChild("Right Leg")
	if LeftLeg then
		LeftShoe = Shoe:Clone()
		LeftShoe.Name = "LeftShoe"
		LeftShoe.Mesh.MeshId = (BaseUrl .. "257655640")
		local LeftWeld = Create("Weld"){
			Part0 = LeftLeg,
			Part1 = LeftShoe,
			C0 = CFrame.new(0, -0.59, -0.45),
			Parent = LeftShoe, 
		}
		LeftShoe.Parent = Tool
	end
	if RightLeg then
		RightShoe = Shoe:Clone()
		RightShoe.Name = "RightShoe"
		RightShoe.Mesh.MeshId = (BaseUrl .. "257655609")
		local RightWeld = Create("Weld"){
			Part0 = RightLeg,
			Part1 = RightShoe,
			C0 = CFrame.new(0, -0.59, -0.45),
			Parent = RightShoe,
		}
		RightShoe.Parent = Tool
	end
	Spawn(function()
		local ParticleParts = {LeftShoe.ParticlePart, RightShoe.ParticlePart}
		while #ParticleParts > 0 and ToolEquipped and CheckIfAlive() do
			local Enabled = ((Torso.Velocity * Vector3.new(1, 0, 1)).Magnitude > 10)
			local Particles = {}
			for i, v in pairs(ParticleParts) do
				if not v or not v.Parent then
					table.remove(ParticleParts, i)
				else
					for ii, vv in pairs(v:GetChildren()) do
						if vv:IsA("Sparkles") or vv:IsA("Fire") or vv:IsA("Smoke") or vv:IsA("Light") then
							local Color = Color3.new((0.5 + math.random()), (0.5 + math.random()), (0.5 + math.random()))
							vv.Color = Color
							if vv:IsA("Sparkles") then
								vv.SparkleColor = Color
							end
							vv.Enabled = Enabled
							table.insert(Particles, vv)
						end
					end
				end
			end
			local Time = tick()
			if (Time - LastSparkle) > Rate then
				if Enabled then
					local RayHit, RayPos, RayNormal = CastRay(Torso.Position, CFrame.new(Torso.Position, (Torso.CFrame * CFrame.new(0, -1, 0)).p).lookVector, ((Torso.Size.Y * 1.5) + 0.2), {Character}, false)
					if RayHit then
						LastSparkle = Time
						local ParticlePart = BasePart:Clone()
						ParticlePart.Size = Vector3.new(1.5, 1.5, 1.5)
						ParticlePart.Name = "Effect"
						ParticlePart.Transparency = 1
						ParticlePart.Anchored = true
						ParticlePart.CanCollide = false
						local Creator = Create("ObjectValue"){
							Name = "Creator",
							Value = Player,
							Parent = ParticlePart,
						}
						if #Particles > 0 then
							for i = 1, 2 do
								local ParticleNumber = math.random(1, #Particles)
								local ParticleSelected = Particles[ParticleNumber]
								table.remove(Particles, ParticleNumber)
								local Particle = ParticleSelected:Clone()
								Particle.Enabled = true
								Particle.Parent = ParticlePart
							end
							local SparkleScriptCopy = SparkleScript:Clone()
							SparkleScriptCopy.Disabled = false
							SparkleScriptCopy.Parent = ParticlePart
							ParticlePart.CFrame = (CFrame.new(RayPos) * CFrame.Angles(0, (math.random(-360, 360) * 0.01), 0))
							Debris:AddItem(ParticlePart, 5)
							ParticlePart.Parent = game:GetService("Workspace")
						end
					end
				end
			end
			wait(0.5)
		end
	end)
end

function CheckIfAlive()
	return (((Player and Player.Parent and Character and Character.Parent and Humanoid and Humanoid.Parent and Humanoid.Health > 0 and Torso and Torso.Parent) and true) or false)
end

function Equipped()
	Character = Tool.Parent
	Player = Players:GetPlayerFromCharacter(Character)
	Humanoid = Character:FindFirstChild("Humanoid")
	Torso = Character:FindFirstChild("Torso")
	if not CheckIfAlive() then
		return
	end
	Humanoid.WalkSpeed = 24
	Handle.Transparency = 1
	Spawn(function()
		EquipShoes(true)
	end)
	ToolEquipped = true
end

function Unequipped()
	ToolEquipped = false
	Handle.Transparency = 0
	Spawn(function()
		EquipShoes(false)
	end)
	if Humanoid and Humanoid.Parent then
		Humanoid.WalkSpeed = 16
	end
end

EquipShoes(false)

Tool.Equipped:connect(Equipped)
Tool.Unequipped:connect(Unequipped)]]></ProtectedString>
