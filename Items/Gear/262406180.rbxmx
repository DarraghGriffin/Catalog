					<ProtectedString name="Source"><![CDATA[--Made by Luckymaxer

Part = script.Parent

Players = game:GetService("Players")
Debris = game:GetService("Debris")

ParticleScript = script:WaitForChild("ParticleScript")

Creator = Part:FindFirstChild("Creator")

Particles = {}

PartsHit = {}

Enabled = true

function IsTeamMate(Player1, Player2)
	return (Player1 and Player2 and not Player1.Neutral and not Player2.Neutral and Player1.TeamColor == Player2.TeamColor)
end

for i, v in pairs(Part:GetChildren()) do
	if v:IsA("Sparkles") or v:IsA("Fire") or v:IsA("Smoke") or v:IsA("Light") then
		table.insert(Particles, v)
	end
end

Part.Touched:connect(function(Hit)
	if not Hit or not Hit.Parent or not Enabled then
		return
	end
	local CreatorPlayer = (((Creator.Value and Creator.Value:IsA("Player")) and Creator.Value) or nil)
	local character = Hit.Parent
	if character:IsA("Hat") then
		character = character.Parent
	end
	local humanoid = character:FindFirstChild("Humanoid")
	if not humanoid or humanoid.Health == 0 then
		return
	end
	local player = Players:GetPlayerFromCharacter(character)
	if CreatorPlayer and player and (CreatorPlayer == player or IsTeamMate(CreatorPlayer, player)) then
		return
	end
	for i, v in pairs(PartsHit) do
		if v == Hit then
			return
		end
	end
	table.insert(PartsHit, Hit)
	for i, v in pairs(Particles) do
		if v and v.Parent and v.Enabled then
			local Particle = v:Clone()
			Particle.Enabled = true
			local ParticleScriptCopy = ParticleScript:Clone()
			ParticleScriptCopy.Disabled = false
			ParticleScriptCopy.Parent = Particle
			Debris:AddItem(Particle, 5)
			Particle.Parent = Hit
		end
	end
end)

Delay(3.5, (function()
	Enabled = false
	for i, v in pairs(Particles) do
		v.Enabled = false
	end
end))

Debris:AddItem(Part, 5)]]></ProtectedString>
