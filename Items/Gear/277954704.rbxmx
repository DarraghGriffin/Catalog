		return
	end
	Explode()
end)	

local AheadVector = (Direction.unit * TotalDist)
local FlightVector = (TargetPosition - Origin)

Spawn(function()
	while Projectile and Projectile.Parent and (Projectile.Position - TargetPosition).magnitude > 1 do
		wait()
	end
	Explode()
end)]]></ProtectedString>
