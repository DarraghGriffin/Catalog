		Humanoid = Humanoid,
		Torso = Torso,
		Target = Target,
		ChaseRange = ChaseRange,
		OriginalChaseRange = ChaseRange.Value,
		TargetPlayer = nil,
		CobWebs = {Tick = tick(), Delay = CobWebDelay},
	}
	table.insert(Spiders, Table)
	ControlSpiders()
end

