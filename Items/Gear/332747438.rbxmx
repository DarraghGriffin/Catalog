		<Item class="LocalScript" referent="RBX94265EF330B0403CA4CC4E0EEA937F5B">
			<Properties>
				<bool name="Disabled">false</bool>
				<Content name="LinkedSource"><null></null></Content>
				<string name="Name">MouseIcon</string>
				<ProtectedString name="Source"><![CDATA[--Made by Luckymaxer

Mouse_Icon = "rbxasset://textures/GunCursor.png"
Reloading_Icon = "rbxasset://textures/GunWaitCursor.png"

Tool = script.Parent

Mouse = nil

function UpdateIcon()
	if Mouse then
		Mouse.Icon = Tool.Enabled and Mouse_Icon or Reloading_Icon
	end
end

function OnEquipped(ToolMouse)
	Mouse = ToolMouse
	UpdateIcon()
end

function OnChanged(Property)
	if Property == "Enabled" then
		UpdateIcon()
	end
end

Tool.Equipped:connect(OnEquipped)
Tool.Changed:connect(OnChanged)
]]></ProtectedString>
			</Properties>
		</Item>
		<Item class="Script" referent="RBX71A9B0AADE8844FBA7803C1AB898C7E1">
			<Properties>
				<bool name="Disabled">false</bool>
				<Content name="LinkedSource"><null></null></Content>
				<string name="Name">Script</string>
				<ProtectedString name="Source"><![CDATA[--Made by Luckymaxer

Tool = script.Parent
Handle = Tool:WaitForChild("Handle")

Players = game:GetService("Players")
Debris = game:GetService("Debris")

RbxUtility = LoadLibrary("RbxUtility")
Create = RbxUtility.Create

BaseUrl = "http://www.roblox.com/asset/?id="

Functions = require(script:WaitForChild("Functions"))

ThrowVelocity = {Min = 0.1, Max = 0.4, Boost = Vector3.new(0, 25, 0)}
PopSounds = {114745014, 114745028, 114745043, 114745060, 114745081}

BasePart = Create("Part"){
	Shape = Enum.PartType.Block,
	Material = Enum.Material.Plastic,
	TopSurface = Enum.SurfaceType.Smooth,
	BottomSurface = Enum.SurfaceType.Smooth,
	FormFactor = Enum.FormFactor.Custom,
	Size = Vector3.new(0.2, 0.2, 0.2),
	CanCollide = true,
	Locked = true,
	Anchored = false,
}

PlungerScript = script:WaitForChild("PlungerScript")

Effects = script:WaitForChild("Effects")

Animations = {
	Hold = {Animation = Tool:WaitForChild("Hold"), FadeTime = nil, Weight = nil, Speed = nil},
	Spin = {Animation = Tool:WaitForChild("Spin"), FadeTime = 0, Weight = nil, Speed = 0.25},
}

Sounds = {
	Spin = Handle:WaitForChild("Spin"),
	Swish = Handle:WaitForChild("Swish"),
	Hit = Handle:WaitForChild("Hit"),
}

ReloadTime = 3

Spinning = false
EffectsOn = false
ToolEquipped = false

Gravity = 196.20
Rate = (1 / 30)

BasePlunger = Handle:Clone()
BasePlunger.Name = "Plunger"
--BasePlunger.Friction = 2
--BasePlunger.Elasticity = 1

ServerControl = (Tool:FindFirstChild("ServerControl") or Create("RemoteFunction"){
	Name = "ServerControl",
	Parent = Tool,
})

ClientControl = (Tool:FindFirstChild("ClientControl") or Create("RemoteFunction"){
	Name = "ClientControl",
	Parent = Tool,
})

Handle.Transparency = 0
Tool.Enabled = true

for i, v in pairs(Tool:GetChildren()) do
	if v:IsA("BasePart") and v ~= Handle then
		v:Destroy()
	end
end

for i, v in pairs(Handle:GetChildren()) do
	if v:IsA("Fire") or v:IsA("Smoke") or v:IsA("Light") or v:IsA("Sparkles") or v:IsA("ParticleEmitter") then
		v:Destroy()
	end
end

function DualWield(CreateNew)
	for i, v in pairs(Tool:GetChildren()) do
		if v:IsA("BasePart") and v ~= Handle then
			v:Destroy()
		end
	end
	if not CreateNew or not LeftArm or not LeftArm.Parent then
		return
	end
	FakeHandle = Handle:Clone()
	FakeHandle.Name = "FakeHandle"
	local FakeHandleWeld = Create("Weld"){
		Part0 = LeftArm,
		Part1 = FakeHandle,
		C0 = CFrame.new(0, -1, 0, 1, 0, -0, 0, 0, 1, 0, -1, -0),
		C1 = Tool.Grip,
		Parent = FakeHandle,
	}
	FakeHandle.Parent = Tool
end

function CheckIfAlive()
	return (((Player and Player.Parent and Character and Character.Parent and Humanoid and Humanoid.Parent and Humanoid.Health > 0 and Torso and Torso.Parent) and true) or false)
end

function Equipped()
	Character = Tool.Parent
	Player = Players:GetPlayerFromCharacter(Character)
	Humanoid = Character:FindFirstChild("Humanoid")
	Torso = Character:FindFirstChild("Torso")
	if not CheckIfAlive() then
		return
	end
	Spawn(function()
		Humanoid.WalkSpeed = 24
		Spawn(function()
			InvokeClient("PlayAnimation", Animations.Hold)
		end)
		LeftArm = Character:FindFirstChild("Left Arm")
		RightArm = Character:FindFirstChild("Right Arm")
		for i, v in pairs({LeftArm, RightArm}) do
			if v then
				Spawn(function()
					InvokeClient("SetLocalTransparencyModifier", {Object = v, Transparency = 0, AutoUpdate = false})
				end)
			end
		end
		DualWield(true)
	end)
	ToolEquipped = true
end

function Unequipped()
	if CheckIfAlive() then
		Humanoid.WalkSpeed = 16
	end
	DualWield(false)
	for i, v in pairs(Sounds) do
		v:Stop()
	end
	EffectsOn = false
	Spinning = false
	ToolEquipped = false
end

function InvokeClient(Mode, Value)
	local ClientReturn = nil
	pcall(function()
		ClientReturn = ClientControl:InvokeClient(Player, Mode, Value)
	end)
	return ClientReturn
end

function OnServerInvoke(player, Mode, Value)
	if player ~= Player or not ToolEquipped or not CheckIfAlive() or not Mode or not Value then
		return
	end
	if Mode == "KeyframeChanged" then
		local Keyframe = Value.Keyframe
		if not Keyframe then
			return
		end
		for i, v in pairs(Limbs) do
			if v.Limb and v.Joint and (v.Joint.Part0 == v.Limb or v.Joint.Part1 == v.Limb) and v.Object and v.Object.Parent then
				local Plunger = BasePlunger:Clone()
				Plunger.CanCollide = false
				Plunger.CFrame = (CFrame.new(v.Object.CFrame.p) * CFrame.Angles(Torso.CFrame:toEulerAnglesXYZ()))
				if (Keyframe == "Start" and v.Limb.Name == "Right Arm") or (Keyframe == "Middle" and v.Limb.Name == "Left Arm") then
					local PX, PY, PZ, R00, R01, R02, R10, R11, R12, R20, R21, R22 = Torso.CFrame:components()
					Plunger.Velocity = (CFrame.new(Plunger.Position.X, Plunger.Position.Y, Plunger.Position.Z, R00, R01, R02, R10, R11, R12, R20, R21, R22).lookVector * (75 + (250 * DesiredThrowVelocity ^ 2)) + ThrowVelocity.Boost)
					if EffectsOn then
						local Colors = {"Really red", "Dark green", "White"}
						local Color = BrickColor.new(Colors[math.random(1, #Colors)]).Color
						for ii, vv in pairs(Effects:GetChildren()) do
							local Effect = vv:Clone()
							if Effect:IsA("ParticleEmitter") then
								Effect.Color = ColorSequence.new(Color, Color)
							else
								Effect.Color = Color
							end
							Effect.Enabled = true
							Effect.Parent = Plunger
						end
					end
					local Sound = Sounds.Hit:Clone()
					Sound.SoundId = (BaseUrl .. PopSounds[math.random(1, #PopSounds)])
					Sound.Parent = Plunger
					local PlungerScriptClone = PlungerScript:Clone()
					local Values = {
						{Name = "Creator", Class = "ObjectValue", Value = Player},
						{Name = "Damage", Class = "NumberValue", Value = 14},
					}
					for i, v in pairs(Values) do
						local Value = Create(v.Class){
							Name = v.Name,
							Value = v.Value,
							Parent = Plunger
						}
					end
					PlungerScriptClone.Disabled = false
					PlungerScriptClone.Parent = Plunger
					local Sound = Sounds.Swish:Clone()
					Sound.Pitch = (1.1 + (0.15 * math.random()))
					Sound.Parent = Plunger
					Debris:AddItem(Plunger, 6)
					Plunger.Parent = game:GetService("Workspace")
					Delay(0.25, (function()
						Plunger.CanCollide = true
					end))
					Sound:Play()
					v.Object.Transparency = 1
				else
					v.Object.Transparency = 0
				end
			end
		end
	elseif Mode == "MouseClick" then
		local Down = Value.Down
		if Down and Tool.Enabled and not Spinning then
			Spinning = true
			Sounds.Spin.Pitch = 1
			Sounds.Spin:Stop()
			Spawn(function()
				local StartTime = tick()
				local EndTime = (StartTime + 2.5)
				local MaxSpeed = 1.25
				EffectsOn = false
				local Animation = Animations.Spin
				DesiredSpeed = Animation.Speed
				DesiredThrowVelocity = ThrowVelocity.Min
				Limbs = {
					{Limb = Character:FindFirstChild("Left Arm"), Joint = Torso:FindFirstChild("Left Shoulder"), Object = FakeHandle},
					{Limb = Character:FindFirstChild("Right Arm"), Joint = Torso:FindFirstChild("Right Shoulder"), Object = Handle},
				}
				while ToolEquipped and Spinning and CheckIfAlive() do
					local CurrentTime = tick()
					local CurrentCharge = math.abs(CurrentTime - StartTime)
					local ChargePercent = Functions.Clamp((Functions.GetPercentage(StartTime, EndTime, CurrentTime) * 0.5), 0, 100)
					local ChargePercentToDecimal = (ChargePercent * 0.01)
					DesiredSpeed = Functions.Clamp((MaxSpeed * ChargePercentToDecimal), Animation.Speed, MaxSpeed)
					DesiredThrowVelocity = Functions.Clamp((ThrowVelocity.Max * ChargePercentToDecimal), ThrowVelocity.Min, ThrowVelocity.Max)
					if DesiredSpeed >= 0.75 and not Sounds.Spin.IsPlaying then
						Sounds.Spin:Play()
					end
					if ChargePercentToDecimal >= 0.5 and not EffectsOn then
						EffectsOn = true
						--------------------------------------------------
					end
					Sounds.Spin.Pitch = Functions.Clamp((1 * ChargePercentToDecimal), 1, 1.75)
					Spawn(function()
						InvokeClient("PlayAnimation", {Animation = Animation.Animation, FadeTime = Animation.FadeTime, Weight = Animation.Weight, Speed = DesiredSpeed, KeyframeMonitor = true})
					end)
					wait(Rate)
				end
				EffectsOn = false
				Sounds.Spin:Stop()
