	local NewCFrame = CFrame.new(Camera.Focus.p) * CameraRotation * CFrame.Angles((math.random(-10, 10) / 40), (math.random(-10, 10) / 40), 0)
	Camera.CoordinateFrame = NewCFrame * CFrame.new(0, 0, CameraScroll)
