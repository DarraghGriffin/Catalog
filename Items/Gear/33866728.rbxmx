				<string name="Name">DrumScript</string>
				<ProtectedString name="Source">local Tool = script.Parent
local keyDown, keyUp
local gui = Tool.DrumGui
local playerGui = nil
local humanoid = nil
local head = nil

function stopAllSound()

	local sounds = Tool.Handle.Sounds:GetChildren()
	for i =1, #sounds do
		sounds[i]:Stop()
	end

end

function onEquipped(newMouse)

	humanoid = Tool.Parent:FindFirstChild(&quot;Humanoid&quot;)
	head =  Tool.Parent:FindFirstChild(&quot;Head&quot;)

	wait(0.4)
	if humanoid and head then
		humanoid.Sit = true
		head.Anchored = true
	end

	keyDown = newMouse.KeyDown:connect(onKeyDown)
	keyUp =  newMouse.KeyUp:connect(onKeyUp)
	
	stopAllSound()

	playerGui = gui:clone()
	playerGui.Parent = game.Players:GetPlayerFromCharacter(Tool.Parent).PlayerGui

end

Tool.Equipped:connect(onEquipped)

function onUnequipped()

	if humanoid and head then
		humanoid.Sit = false
		head.Anchored = false
	end

	keyDown:disconnect()
	keyUp:disconnect()

	stopAllSound()

	playerGui:remove()
	playerGui = nil

end

Tool.Unequipped:connect(onUnequipped)

function onKeyDown(key)

	key:lower()
	if playerGui.Frame:FindFirstChild(key) ~= nil then
		playerGui.Frame:FindFirstChild(key).Visible = true
	end
	if key == &quot;g&quot; then
		Tool.Handle.Sounds.HiHat:Play()
	elseif key == &quot;h&quot; then
		Tool.Handle.Sounds.Snare:Play()
	elseif key == &quot;j&quot; then
		Tool.Handle.Sounds.Tom2:Play()
	elseif key == &quot;k&quot; then
		Tool.Handle.Sounds.Tom3:Play()
	elseif key == &quot;y&quot; then
		Tool.Handle.Sounds.Crash:Play()
	elseif key == &quot;u&quot; then
		Tool.Handle.Sounds.Tom1:Play()
	elseif key == &quot;t&quot; then
		Tool.Handle.Sounds.Ride:Play()
	elseif key == &quot;n&quot; then
		Tool.Handle.Sounds.Kick:Play()
	end

end

function onKeyUp(key)

	key:lower()
	if playerGui.Frame:FindFirstChild(key) ~= nil then
		playerGui.Frame:FindFirstChild(key).Visible = false
	end

end


