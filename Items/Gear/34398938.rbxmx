				<ProtectedString name="Source"><![CDATA[--Rescripted by Luckymaxer

Tool = script.Parent
Handle = Tool:WaitForChild("Handle")

Players = game:GetService("Players")
Debris = game:GetService("Debris")

PinataScript = script:WaitForChild("PinataScript")

ToolEquipped = false

function PinataName()
	if not CheckIfAlive() then
		return
	end
	return (Player.Name .. "'s Pinata")
end

function PinataExists()
	if not CheckIfAlive() then
		return false
	end
	return game:GetService("Workspace"):FindFirstChild(PinataName())
end

function Activated()
	if not ToolEquipped or not CheckIfAlive() or not Tool.Enabled or PinataExists() then
		return
	end
	
	local PinataModel = Instance.new("Model")
	PinataModel.Name = PinataName()
	
	local Pinata = Handle:clone()
	Pinata.Name = "Head"
	Pinata.CanCollide = true
	Pinata.Size = Vector3.new(3, 3, 1)
	Pinata.CFrame = CFrame.new((RootPart.Position.x + (RootPart.CFrame.lookVector.Unit.X * 8)), RootPart.Position.Y, (RootPart.Position.Z + (RootPart.CFrame.lookVector.Unit.Z * 8)))
	Pinata.Parent = PinataModel
	
	local PinataNameTag = Instance.new("Humanoid")
	PinataNameTag.MaxHealth = 0
	PinataNameTag.Health = 0
	PinataNameTag.Parent = PinataModel
	
	local Creator = Instance.new("ObjectValue")
	Creator.Name = "Creator"
	Creator.Value = Player
	Creator.Parent = PinataModel
	
	local Health = Instance.new("NumberValue")
	Health.Name = "Health"
	Health.Value = 100
	Health.Parent = PinataModel
	
	local BodyPos = Instance.new("BodyPosition")
	BodyPos.position = (Pinata.CFrame * CFrame.new(0, 5, 0)).p
	BodyPos.P = 8000
	BodyPos.D = BodyPos.P
	BodyPos.MaxForce = Vector3.new(BodyPos.P, BodyPos.P, BodyPos.P)
	BodyPos.Parent = Pinata

	local BodyGyro = Instance.new("BodyGyro")
	BodyGyro.P = 50
	BodyGyro.D = BodyGyro.P
	BodyGyro.MaxTorque = Vector3.new(BodyGyro.P, 0, 0)
	BodyGyro.CFrame = Pinata.CFrame
	BodyGyro.Parent = Pinata
	
	local PinataScriptCopy = PinataScript:clone()
	PinataScriptCopy.Disabled = false
	PinataScriptCopy.Parent = PinataModel
	
	Debris:AddItem(PinataModel, (60 * 3))
	
	PinataModel.Parent = game:GetService("Workspace")
	
	Tool:Destroy()
	
end

function CheckIfAlive()
	return (((Player and Player.Parent and Character and Character.Parent and Humanoid and Humanoid.Health > 0 and RootPart and RootPart.Parent) and true) or false)
end

function Equipped()
	Character = Tool.Parent
	Player = Players:GetPlayerFromCharacter(Character)
	Humanoid = Character:FindFirstChild("Humanoid")
	RootPart = Character:FindFirstChild("HumanoidRootPart")
	if not CheckIfAlive() then
		return
	end
	ToolEquipped = true
end

function Unequipped()
	ToolEquipped = false
end

Tool.Activated:connect(Activated)
Tool.Equipped:connect(Equipped)
Tool.Unequipped:connect(Unequipped)]]></ProtectedString>
