					<ProtectedString name="Source"><![CDATA[--Rescripted by Luckymaxer

Pinata = script.Parent
Part = Pinata:WaitForChild("Head")
Mesh = Part:WaitForChild("Mesh")

InsertService = game:GetService("InsertService")
Debris = game:GetService("Debris")

Creator = Pinata:FindFirstChild("Creator")
Health = Pinata:WaitForChild("Health")

Module = require(191816425)

BasePart = Instance.new("Part")
BasePart.Material = Enum.Material.Plastic
BasePart.Shape = Enum.PartType.Block
BasePart.TopSurface = Enum.SurfaceType.Smooth
BasePart.BottomSurface = Enum.SurfaceType.Smooth
BasePart.FormFactor = Enum.FormFactor.Custom
BasePart.Size = Vector3.new(0.2, 0.2, 0.2)
BasePart.Locked = true
BasePart.CanCollide = true
BasePart.Anchored = false

Sounds = {
	Break = Part:FindFirstChild("Break"),
}

Breaking = false
Touching = false

function GetCreator()
	return (((Creator and Creator.Value and Creator.Value.Parent and Creator.Value:IsA("Player")) and Creator.Value) or nil)
end

CreatorPlayer = GetCreator()
ModuleTable = Module.GetTable({Key = "BirthdayPinata", Player = CreatorPlayer})
ModuleFunctions = ModuleTable.GetData({Player = CreatorPlayer})

Scale = 1
Increment = 0.1
while Scale < 1.6 do
	Mesh.Scale = Vector3.new(Scale, Scale, Scale)
	Scale = (Scale + Increment)
	wait()
end

function InsertAsset(AssetId)
	local AssetModel = InsertService:LoadAsset(AssetId)
	local Asset = AssetModel:GetChildren()[1]:Clone()
	Debris:AddItem(AssetModel, 0.5)
	return Asset
end

function Burst()
	local Colors = {"Bright red", "Bright yellow", "Bright orange"}
	for i = 1, 20 do
		local Confetti = BasePart:Clone()
		Confetti.Name = "Effect"
		Confetti.Size = Vector3.new(1, 0.4, 1)
		Confetti.BrickColor = BrickColor.new(Colors[math.random(1, #Colors)])
		Confetti.Material = Enum.Material.Grass
		Confetti.CFrame = (Part.CFrame * CFrame.new(math.random(-1, 1), math.random(-1, 1), math.random(-1, 1)))
		Debris:AddItem(Confetti, (4 + (math.random(50, 200) * 0.01)))
		Confetti.Parent = game:GetService("Workspace")
	end
	
	local Gears = ModuleFunctions.GetGearIds()
	for i, v in pairs(Gears) do
		local Gear = InsertAsset(v)
		local Handle = Gear:FindFirstChild("Handle")
		if Handle then
			Handle.CFrame = (Part.CFrame * CFrame.new(math.random(-1, 1), math.random(-1, 1), math.random(-1, 1)))
			Gear.Parent = game:GetService("Workspace")
		end
	end
end

function CheckHealth()
	if Health.Value > 0 or Breaking then
		return
	end
	Breaking = true
	Burst()
	Part.Transparency = 1
	Part.CanCollide = false
	Part.Anchored = true
	Part.Name = "Effect"
	Sounds.Break:Play()
	Debris:AddItem(Pinata, 2)
	
end

function Touched(Hit)
	if not Hit or not Hit.Parent or not Part.CanCollide or Touching or Breaking then
		return
	end
	Touching = true
	Health.Value = (Health.Value - (Hit.Velocity.Magnitude / 50))
	wait(0.1)
	Touching = false
end

Health.Changed:connect(function()
	CheckHealth()
end)

Part.Touched:connect(Touched)
]]></ProtectedString>
