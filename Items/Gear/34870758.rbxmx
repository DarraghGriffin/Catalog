				<string name="Name">PortalShoot</string>
				<ProtectedString name="Source">Tool = script.Parent
local portals = 0
local activePortals = 0
local workPortals = nil

function fire(v)

	Tool.Handle.Fire:play()
	local touched = false

	local vCharacter = Tool.Parent
	local vPlayer = game.Players:playerFromCharacter(vCharacter)

	Tool.Parent.Torso[&quot;Right Shoulder&quot;].DesiredAngle = 3.6
	Tool.Parent.Torso[&quot;Right Shoulder&quot;].MaxVelocity = 0.5

	local missile = Instance.new(&quot;Part&quot;)

	local m = Tool.Mesh:Clone()	
	m.Parent = missile

	local spawnPos = vCharacter.PrimaryPart.Position

	spawnPos  = spawnPos + (v * 8)

	missile.CFrame = CFrame.new(spawnPos, spawnPos + v)
	missile.Position = spawnPos
	missile.Size = Vector3.new(1,1,1)
	missile.Velocity = v * 80
	missile.BrickColor = BrickColor.new(23)
	missile.Shape = 0
	missile.BottomSurface = 0
	missile.TopSurface = 0
	missile.Name = tostring(portals)
	missile.Elasticity = 0
	missile.Reflectance = 0
	missile.Friction = .4

	local force = Instance.new(&quot;BodyForce&quot;)
	force.force = Vector3.new(0,102,0)
	force.Parent = missile

	local script = Tool.PortalScript:clone()
	script.Parent = missile
	script.Disabled = false

	local creator_tag = Instance.new(&quot;ObjectValue&quot;)
	creator_tag.Value = vPlayer
	creator_tag.Name = &quot;creator&quot;
	creator_tag.Parent = missile

	local portalSound = Instance.new(&quot;Sound&quot;)
	portalSound.SoundId = &quot;http://www.roblox.com/asset/?id=34320976&quot;
	portalSound.Parent = missile

	missile.Parent = workPortals

	local con = missile.Touched:connect(function(part)
		if part.Parent:FindFirstChild(&quot;Humanoid&quot;) == nil and not touched then
			portals = portals + 1
			updatePortals(missile)
			touched = true
		end
	end)
	wait(3)
	con:disconnect()
	if not touched then
		print(&quot;removed&quot;)
		missile:remove()
	end

end

function updatePortals(missile)

	activePortals = activePortals + 1
	if activePortals &gt; 2 then
		local portalToKill = missile
		local stuff = workPortals:GetChildren()
		for i = 1, #stuff do
			if stuff[i].className ~= &quot;BoolValue&quot; then
				if tonumber(stuff[i].Name) &lt; tonumber(portalToKill.Name) then
					portalToKill = stuff[i]
				end
			end
		end
		portalToKill:remove()
	end

end


Tool.Enabled = true
function onActivated()

	--[[if not Tool.Enabled then
		return
	end

	Tool.Enabled = false]]

	local character = Tool.Parent;
	local humanoid = character.Humanoid
	if humanoid == nil then
		print(&quot;Humanoid not found&quot;)
		return 
	end

	local targetPos = humanoid.TargetPoint
	local lookAt = (targetPos - character.Head.Position).unit

	fire(lookAt)

	--Tool.Enabled = true
end


script.Parent.Activated:connect(onActivated)

function onEquipped()

	workPortals = Instance.new(&quot;Model&quot;)
	workPortals.Name = Tool.Parent.Name ..&quot;Portals&quot;
	workPortals.Parent = game.Workspace

	local portalValue = Instance.new(&quot;BoolValue&quot;)
	portalValue.Name = &quot;Portaling&quot;
	portalValue.Parent = workPortals

	portals = 0
	activePortals = 0

end

function onUnequipped()
	workPortals:remove()
end

Tool.Equipped:connect(onEquipped)
Tool.Unequipped:connect(onUnequipped)
