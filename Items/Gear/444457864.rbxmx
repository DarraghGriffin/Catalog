	Functions.UntagHumanoid(humanoid)
	Functions.TagHumanoid(humanoid, CreatorPlayer)
	humanoid:TakeDamage(Damage.Value)
	wait(0.2)
	for i, v in pairs(PlayersHit) do
		if v == character then
			table.remove(PlayersHit, i)
		end
	end
	--HitDelay = false
end

StartTick = tick()
PlayersHit = {}
LastStrike = 0
StrikeDelay = 0.2

