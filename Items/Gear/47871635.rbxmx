				<ProtectedString name="Source">local Tool = script.Parent 
local Tuba = script.Parent.Handle

Tool.Enabled = true

-- Sounds
local TubaSound 
--

-- Animations 
local tubaAnim
--
-- Clone
local tubaClone


function shoot(pos)
	
	local vCharacter = Tool.Parent
	if vCharacter == nil or vCharacter:FindFirstChild(&quot;Humanoid&quot;) == nil then 
		--print(&quot;HUMANOID NOT FOUND&quot;)
		return 
	end

	local vPlayer = game.Players:GetPlayerFromCharacter(vCharacter)
	if vPlayer == nil then print(&quot;PLAYER NOT FOUND&quot;) end
	local spawnPos = vCharacter.PrimaryPart.Position
	print(spawnPos)
	print(pos)
	spawnPos = spawnPos + pos.unit * 5.0
	ShockwaveC = script:FindFirstChild(&quot;ShockWave&quot;):Clone()

	--ShockwaveC.Transparency = 0.0
	ShockwaveC.CanCollide = false
	ShockwaveC.Anchored = false
	ShockwaveC:FindFirstChild(&quot;ShockwaveScript&quot;).Disabled = false
	ShockwaveC.Size = Vector3.new(8, 0.4, 8)
	ShockwaveC.formFactor = 2

	local decal = Instance.new(&quot;Decal&quot;)
	decal.Specular = 20
	decal.Face = 1
	decal.Texture = &quot;http://www.roblox.com/asset/?version=1&amp;id=1280730&quot;
	decal.Parent = ShockwaveC

	local bottomDecal = decal:Clone()
	bottomDecal.Specular = 20
	bottomDecal.Face = 4
	bottomDecal.Parent = ShockwaveC

	ShockwaveC.CFrame = CFrame.new(spawnPos, spawnPos +pos) * CFrame.Angles(math.pi/2, 0.0, 0.0)
	ShockwaveC.Velocity = pos * 25.0
	--ShockwaveC.RotVelocity = Vector3.new(0.0, 0.0, 0.0)
	ShockwaveC.Name = &quot;ShockwavePowa&quot;
	ShockwaveC.Parent = game.Workspace	
	
	ShockwaveC.CanCollide = true

	local ShockwaveFloat = Instance.new(&quot;BodyForce&quot;)
	ShockwaveFloat.force = Vector3.new(0.0, (ShockwaveC:GetMass()) * 196.1, 0.0)
	ShockwaveFloat.Parent = ShockwaveC

	--connection = ShockwaveC.Touched:connect(blow)
end

function onActivated()
	if Tool.Enabled == false then 
		return
	end

	local character = Tool.Parent

	Tool.Enabled = false

	if character == nil then 
		print(&quot;CHARACTER NOT FOUND&quot;)
		return 
	end

	myHumanoid = Tool.Parent:FindFirstChild(&quot;Humanoid&quot;)
	if myHumanoid == nil then 
		print(&quot;HUMANOID NOT FOUND&quot;)
		return 
	end

	myTorso = Tool.Parent:FindFirstChild(&quot;Torso&quot;)
	if myTorso == nil then 
		print(&quot;TORSO NOT FOUND&quot;)
		return
	end
	
	tubaClone = Tool.Handle:Clone()
	tubaClone.CanCollide = false
	tubaClone.Parent = Workspace
	
	Tuba.Transparency = 1.0
	
	local newWeld = Instance.new(&quot;Weld&quot;)
	newWeld.Part1 = myTorso
	newWeld.Part0 = tubaClone
	newWeld.C1 = CFrame.new(Vector3.new(-0.3, 0.8, 0.0)) * CFrame.Angles(math.pi, 0.0, -math.pi/4)
	newWeld.Parent = myTorso

	
	tubaAnim = myHumanoid:LoadAnimation(Tool.Tuba)
	if tubaAnim then tubaAnim:Play() end
	TubaSound = Tool:FindFirstChild(&quot;TubaSound&quot;)
	if TubaSound then TubaSound:Play()end
	wait(1.75)

	local targetPos = myHumanoid.TargetPoint 
	--local lookAt = (targetPos - character.Head.Position).unit
	lookAt = myTorso.CFrame.lookVector.unit
	shoot(lookAt)

	--myTorso.Anchored = true
	
	wait(1.5)
	targetPos = myHumanoid.TargetPoint 
	--lookAt = (targetPos - character.Head.Position).unit
	lookAt = myTorso.CFrame.lookVector.unit	
	shoot(lookAt)
	wait(1.5)
	targetPos = myHumanoid.TargetPoint 
	--lookAt = (targetPos - character.Head.Position).unit
	lookAt = myTorso.CFrame.lookVector.unit
	shoot(lookAt)
	wait(1.5)
	targetPos = myHumanoid.TargetPoint 
	--lookAt = (targetPos - character.Head.Position).unit
	lookAt = myTorso.CFrame.lookVector.unit
	shoot(lookAt)
	wait(1.0)
	tubaClone:Remove()
	Tuba.Transparency = 0.0

	--myTorso.Anchored = false
	--if bodyGyro then bodyGyro:Remove() end	
	wait(4)
	Tool.Enabled = true	
end

function onEquipped()
end

function onUnequipped()
	if TubaSound then TubaSound:Stop() end
	if tubaAnim then tubaAnim:Stop() end
	if tubaClone then tubaClone:Remove() end
	Tuba.Transparency = 0.0
end

Tool.Activated:connect(onActivated)
Tool.Equipped:connect(onEquipped)
Tool.Unequipped:connect(onUnequipped)
	



	
