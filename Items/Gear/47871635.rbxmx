						<string name="Name">ShockwaveScript</string>
						<ProtectedString name="Source">missile = script.Parent
bodyGyro = Instance.new(&quot;BodyGyro&quot;)
bodyGyro.P = 10000
bodyGyro.cframe = missile.CFrame
bodyGyro.Parent = missile


local debris = game:GetService(&quot;Debris&quot;)function blow(hit)	
	humanoid = hit.Parent:FindFirstChild(&quot;Humanoid&quot;)
	torso = hit.Parent:FindFirstChild(&quot;Torso&quot;)
	if humanoid ~= nil and torso ~=nil then 		
		local bodyVelocity = Instance.new(&quot;BodyVelocity&quot;)
		bodyVelocity.P = 100000
		bodyVelocity.maxForce = Vector3.new(bodyVelocity.P, bodyVelocity.P, bodyVelocity.P)
		bodyVelocity.velocity = (torso.Position - missile.Position) * 12
		bodyVelocity.Parent = torso
		wait(1.0)
		if bodyVelocity ~= nil then bodyVelocity:remove() end
		missile:Remove()
	end
end

connection = missile.Touched:connect(blow)
wait(3)
if bodyGyro then bodyGyro:Remove() end
missile:Remove()</ProtectedString>
