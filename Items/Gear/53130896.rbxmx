				<ProtectedString name="Source">-- Voodoo Doll
-- Can cast Fireball and Iceball

local Tool = script.Parent
local Doll = Tool.Handle

Tool.Enabled = true

local vCharacter 
local myHumanoid

local woohoo 

bb = Instance.new(&quot;BillboardGui&quot;)
bb.Active = false
bb.Size = UDim2.new(1, 0, 1, 0)

txt = Instance.new(&quot;TextLabel&quot;)
txt.Parent = bb
txt.Text = &quot;&quot;
txt.TextColor3 = Color3.new(1, 0, 0)
txt.FontSize = 4

local debris = game:GetService(&quot;Debris&quot;)

ball = Instance.new(&quot;Part&quot;) 
ball.Size = Vector3.new(1, 1, 1) 
ball.Transparency = 0.8
ball.Shape = &quot;Ball&quot; 
ball.FormFactor = 3 
ball.Name =&quot;HC&quot; 
ball.CanCollide = false 

local ballMesh = Instance.new(&quot;SpecialMesh&quot;) 
ballMesh.MeshType = &quot;FileMesh&quot; 
ballMesh.Name = &quot;Mesh&quot; 
ballMesh.MeshId = &quot;http://www.roblox.com/asset/?id=48423624&quot; 
ballMesh.TextureId = &quot;http://www.roblox.com/asset/?id=49040283&quot; 
ballMesh.Scale = Vector3.new(0.05, 0.05, 0.05) 
ballMesh.Parent = ball 

local BF = Instance.new(&quot;BodyForce&quot;) 
BF.force = Vector3.new(0, ball:GetMass() * 196.1, 0)
BF.Name = &quot;BF&quot; 

function blow(hit, ballClone)
	if hit.Parent then
		humanoid = hit.Parent:FindFirstChild(&quot;Humanoid&quot;)
		if humanoid then 			
			newScript = hit.Parent:FindFirstChild(&quot;VoodooScript&quot;)
			if newScript == nil then
				newScript = Tool.VoodooScript:Clone()
				newScript.Parent = hit.Parent
				newScript.Disabled = false		

				tag = ballClone:FindFirstChild(&quot;Debuff&quot;)
				tag.Parent = hit.Parent
			end
			humanoid:TakeDamage(25)
		end
	end
	if ballClone then ballClone:Remove() end
end

function shoot(pos)
	vCharacter = Tool.Parent
	
	if vCharacter == nil or vCharacter:findFirstChild(&quot;Humanoid&quot;) == nil then
		return
	end
	vPlayer = game.Players:GetPlayerFromCharacter(vCharacter)
	if vPlayer == nil then
		return
	end

	local spawnPos = vCharacter.Torso.Position
	spawnPos = spawnPos + pos.unit * 5.0

	ballClone = ball:Clone()
	ballClone.CFrame = CFrame.new(spawnPos, spawnPos + pos)
	ballClone.Parent = game.Workspace
	ballClone.Velocity = direction * 75.0
	
	ballFloat = BF:Clone()
	ballFloat.Parent = ballClone

	rand = math.random(1, 2)	

	tag = Instance.new(&quot;StringValue&quot;)
	tag.Name = &quot;Debuff&quot;
	if rand == 1 then 		
		tag.Value = &quot;Fire&quot;
		tag.Parent = ballClone

		fire = Instance.new(&quot;Fire&quot;)
		fire.Parent = ballClone
	else
		tag.Value = &quot;Ice&quot;
		tag.Parent = ballClone

		fire = Instance.new(&quot;Fire&quot;)
		fire.Parent = ballClone
		fire.Color = Color3.new(0, 0, 10.75)
	end

	ballClone.Touched:connect(function(hit) blow(hit, ballClone) end)

	debris:addItem(ballClone, 5)
	
end

function onActivated()
	if Tool.Enabled == false then 
		return
	end

	woohoo = Tool:FindFirstChild(&quot;Woohoo&quot;)

	Tool.Enabled = false
	vCharacter = Tool.Parent
	myTorso = vCharacter:FindFirstChild(&quot;Torso&quot;)
	myHumanoid = vCharacter:FindFirstChild(&quot;Humanoid&quot;)

	if myTorso == nil or myHumanoid == nil then 
		return
	end

	bb.Name = &quot;Announcement&quot;	
	bb.Adornee = vCharacter.Head
	bb.StudsOffset = Vector3.new(0.4, 1, 0)

	if woohoo then woohoo:Play() end

	rand = math.random(1, 2)
	if rand == 1 then
		txt.Text = &quot;EXTRA HEALTH!!&quot; 
		ratio = myHumanoid.Health/myHumanoid.MaxHealth
		myHumanoid.MaxHealth = 160 	
		wait(0.1)
		myHumanoid.Health  = ratio * 160
	else	
		txt.Text = &quot;HASTE!!&quot; 
		myHumanoid.WalkSpeed = 22.0
	end

	children = myHumanoid:GetChildren()
	for i = 1, #children do
		if children[i].className == &quot;ObjectValue&quot; and children[i].Name == &quot;creator&quot; then
			enemy = children[i].Value 
			if enemy then
				enemyChar = enemy.Character
				enemyTorso = enemyChar:FindFirstChild(&quot;Torso&quot;)
				if enemyTorso then 
					print(&quot;Shooting my ENEMY&quot;)
					txt.Text = &quot;YOUR ENEMY IS MY ENEMY&quot;
					direction = (enemyTorso.Position - vCharacter.Head.Position).unit 
					shoot(direction)
				end
			end
		end
	end

	wait(1.0)
	txt.Text = &quot;&quot;

	wait(5.0)
	ratio = myHumanoid.Health/myHumanoid.MaxHealth
	myHumanoid.Health = ratio * 100
	myHumanoid.MaxHealth = 100
	myHumanoid.WalkSpeed = 16.0
	wait(2.0)
	Tool.Enabled = true
end

function onEquipped()
	vCharacter = Tool.Parent
	vPlayer = game.Players:GetPlayerFromCharacter(vCharacter)
	if vPlayer then 
		bb.Parent = vPlayer.PlayerGui
	end
end

function onUnequipped()
	if bb then bb:Remove() end

	if myHumanoid then
		ratio = myHumanoid.Health/myHumanoid.MaxHealth
		myHumanoid.Health = ratio * 100
		myHumanoid.MaxHealth = 100
		myHumanoid.WalkSpeed = 16.0
	end

	if woohoo then woohoo:Stop() end
end

Tool.Activated:connect(onActivated)
Tool.Equipped:connect(onEquipped)
Tool.Unequipped:connect(onUnequipped)

