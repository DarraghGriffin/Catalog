				<string name="Name">VoodooScript</string>
				<ProtectedString name="Source">char = script.Parent
humanoid = char:FindFirstChild(&quot;Humanoid&quot;)
torso = char:FindFirstChild(&quot;Torso&quot;)
tag = char:FindFirstChild(&quot;Debuff&quot;)

while tag == nil do wait(1) tag = ball:FindFirstChild(&quot;Debuff&quot;) end

local duration = 5.0

if tag.Value == &quot;Fire&quot; then
	fire = Instance.new(&quot;Fire&quot;)
	if torso then fire.Parent = torso end
	while duration &gt; 0 do 
		if humanoid then humanoid:TakeDamage(5) end
		duration = duration - 1
		wait(1.0)
	end	
else 
	ice = Instance.new(&quot;Fire&quot;)
	ice.Color = Color3.new(0, 0, 0.75)
	if torso then ice.Parent = torso end
	while duration &gt; 0 do 
		humanoid.WalkSpeed = 8.0
		if humanoid then humanoid:TakeDamage(2.0)  end
		duration = duration - 1
		wait(1.0)
	end			
end

humanoid.WalkSpeed = 16.0

wait(1.0)
script:Remove()
