					<ProtectedString name="Source"><![CDATA[local char = script.Parent 

local humanoid = char:FindFirstChild("Humanoid")
local myTorso = char:FindFirstChild("Torso")

local camera = game.Workspace.CurrentCamera

local duration = 3.0

bodyAngularVelocity = Instance.new("BodyAngularVelocity")
bodyAngularVelocity.P = 10000
bodyAngularVelocity.Name = "Tripforce"
bodyAngularVelocity.Parent = myTorso

if humanoid then 
	while duration > 0 do 
		rand = math.random(1, 100)
		rand2 = math.random(2, 50)
		bodyAngularVelocity.angularvelocity = Vector3.new(rand/10, (rand2 * rand)/10, (rand/rand2))
		camera.CameraType = 1		
		humanoid.Sit = not humanoid.Sit
