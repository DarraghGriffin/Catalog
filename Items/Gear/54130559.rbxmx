				<string name="Name">LocalTridentScript</string>
				<ProtectedString name="Source"><![CDATA[-- Handle to the tool
local Tool = script.Parent
-- Handle to the physics object of the tool
local Trident = Tool.Handle

local GLib = require(Tool:WaitForChild'GLib')

-- Animations
local stabAnim
local slashAnim
local quakeAnim

-- Handle to the character and its humanoid object
local vCharacter
local myHumanoid
local myTorso

-- Coroutine to handle the quake cooldown 
local cooldownCo 

-- List of chars to be quaked
local quakedChars = {}

local activateQuake 

-- Body Velocity 
local bodyVelocity = Instance.new("BodyVelocity")
bodyVelocity.P = 10000
bodyVelocity.Name = "QuakeVel"
bodyVelocity.maxForce = Vector3.new(10000, 10000, 10000)

-- Force AOE
local AOE = Vector3.new(15, 15, 15)

-- Animations
local slashAnim = Tool:FindFirstChild("SlashAnim")
local stabAnim =  Tool:FindFirstChild("StabAnim")
local anim
local allAnims = {stabAnim, slashAnim}

QuakeEnabled = Tool:WaitForChild("QuakeEnabled")

-- Services
local uis, cas = game:GetService'UserInputService', game:GetService'ContextActionService'

-- Special function to create custom events 
function guraGura(key)
	-- If x is pressed 
	myTorso = vCharacter:FindFirstChild("Torso")
	activateQuake = Tool:FindFirstChild("ActivateQuake")
	vPlayer = game.Players:GetPlayerFromCharacter(vCharacter)
	myHumanoid = vCharacter:FindFirstChild("Humanoid")
	leftArm = vCharacter:FindFirstChild("Left Arm")
	if key:lower() == "x" then
		if QuakeEnabled.Value then --Fix implemented by Luckymaxer
			QuakeEnabled.Value = false
			-- Reset the array to be nil 
			quakedChars = {}
			quakeAnim = myHumanoid:LoadAnimation(Tool.QuakeAnim)
			if quakeAnim then quakeAnim:Play() end			
			wait(0.75)
			-- Give the go ahead for the quake			
			activateQuake:FireServer(1)
			vPlayer = game.Players:GetPlayerFromCharacter(vCharacter)					
			players = game.Players:GetChildren()
			if #players > 1 then 
				for i = 1, #players do 
					if players[i] ~= vPlayer then 
						tempChar = players[i].Character
						if tempChar then
							tempTorso = tempChar:FindFirstChild("Torso")
							if tempTorso then
								distance = (tempTorso.Position - myTorso.Position).magnitude
								if distance < 100 then
									table.insert(quakedChars, tempChar)
								end
							end
						end
					end
				end
				for i = 1, #quakedChars do 
					script.Parent.QuakeRemote:FireServer(quakedChars[i])
				end		
			end	
			parts = game.Workspace:FindPartsInRegion3(Region3.new(myTorso.Position - AOE, myTorso.Position + AOE), vCharacter, 100)	
			script.Parent.QuakePartRemote:FireServer(parts, myTorso)
			wait(2.0)			
			activateQuake:FireServer(0)
		end
	end
end

function onActivated()
	-- Wait until the tool is enabled again by the serverside script under the tool 
	-- This only works because localscripts are executed client side and so will always be executed faster 
	-- so before the serverside script locks Enabled this function will always skip this loop and go forward 
	while Tool.Enabled == false do wait() end
	vCharacter = Tool.Parent
	myHumanoid = vCharacter:FindFirstChild("Humanoid")
	if myHumanoid == nil then 
		return
	end
	rand = math.random(1, 2)
	anim = myHumanoid:LoadAnimation(allAnims[rand])	
	if anim then anim:Play() end
	if rand == 2 then
		Tool.GripPos = Vector3.new(0, 0, -2.8) 
		wait(1.2)
		Tool.GripPos = Vector3.new(0, 0, -0.6) 
	else
		wait(0.2)
		Tool.GripUp = Vector3.new(0, 0, -1)
		Tool.GripPos = Vector3.new(0, 0, -3.0)
		wait(0.7)
		Tool.GripPos = Vector3.new(0, 0, -0.6) 
		Tool.GripUp = Vector3.new(0, 0, 1)
	end

end

function onEquipped(mouse)
	-- Always nice to get the handle to the character established during the Equipped event
	vCharacter = Tool.Parent
	-- If mouse object is found then connect keyboard events to the GuraGura function
	if mouse then mouse.KeyDown:connect(guraGura) end
	activeQuake = Tool:FindFirstChild("ActivateQuake")
	
	if uis.TouchEnabled then
		local lastQuake = 0
		
		cas:BindActionToInputTypes(
			'PoseidonsQuakeTrident_Quake',
			function(name, state)
				if state == Enum.UserInputState.End then
					if tick() - lastQuake > 5 then
						GLib.SetButtonCooldown('PoseidonsQuakeTrident_Quake', 5)
						lastQuake = tick()
						GLib.FastSpawn(function() guraGura('x') end)
					end
				end
			end,
			true,
			''
		)
		cas:SetTitle('PoseidonsQuakeTrident_Quake', 'Quake')
		cas:SetImage('PoseidonsQuakeTrident_Quake', Tool.TextureId)
	end
end

function onUnequipped()
	-- stop any animation that maybe playing when unequipped event is called
	-- before trying to stop make sure that the animation object exists
	-- for example the variables never get loaded with animations if the tool is never activated 
	-- but it can be equipped and unequipped
	if anim then anim:Stop() end
	
	if uis.TouchEnabled then
		cas:UnbindAction'PoseidonsQuakeTrident_Quake'
	end
end

Tool.Activated:connect(onActivated)
Tool.Equipped:connect(onEquipped)
Tool.Unequipped:connect(onUnequipped)]]></ProtectedString>
