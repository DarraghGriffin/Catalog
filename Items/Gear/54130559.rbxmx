				<ProtectedString name="Source"><![CDATA[-- OMG TRIDENT -- 

-- handle to the tool and its object
local Tool = script.Parent
local Trident = Tool.Handle
local GLib = require(206209239)
GLib.Script.Parent = Tool

-- Handle to the character and its torso
local vCharacter
local myTorso

-- Sounds
local quakeSound

-- damage
local damage = 24

local activateQuake

local quakeSound

-- Access to enabling and disabling the tool as necessary
Tool.Enabled = true
-- handle to the debris(garbage marker) service
local debris = game:GetService("Debris")

local shockw = Instance.new("Part")
shockw.CanCollide = false
shockw.Shape = "Ball"
shockw.Size = Vector3.new(3, 3, 3)
shockw.FormFactor = 3
shockw.Transparency = 0.5

local shockMesh = Instance.new("SpecialMesh")
shockMesh.MeshId = "http://www.roblox.com/asset/?id=54068514"
shockMesh.TextureId = "http://www.roblox.com/asset/?id=54068645"
shockMesh.Scale = Vector3.new(2, 2, 2)
shockMesh.Parent = shockw

local shockw2 = Instance.new("Part")
shockw2.CanCollide = false
shockw2.Shape = "Ball"
shockw2.Size = Vector3.new(3, 3, 3)
shockw2.FormFactor = 3
shockw2.Transparency = 0.5

local shockMesh2= Instance.new("SpecialMesh")
shockMesh2.MeshId = "http://www.roblox.com/asset/?id=54068569"
shockMesh2.TextureId = "http://www.roblox.com/asset/?id=54068645"
shockMesh2.Scale = Vector3.new(2, 2, 2)
shockMesh2.Parent = shockw2

local float = Instance.new("BodyForce")
float.force = Vector3.new(0, shockw:GetMass() * 196.1, 0)
float.Name = "float"
float.Parent = shockw

local float2 = float:Clone()
float2.Parent = shockw2

function quake(a)
	if a == 1 then				
		quakeSound  = Tool:FindFirstChild("QuakeSound")
		if quakeSound then quakeSound:Play() end
		myTorso.Anchored = true
		sw = shockw:Clone()		
		sw.Position = myTorso.Position
		sw.Parent = vCharacter
		sw.RotVelocity = Vector3.new(0, 10, 0)		
		
		sw2 = shockw2:Clone()		
		sw2.Position = myTorso.Position		
		sw2.Parent = vCharacter
		sw2.RotVelocity = Vector3.new(0, -10, 0)	
	
		for i = 1, 20 do 			
			sw.Mesh.Scale = sw.Mesh.Scale + Vector3.new(1, 1, 1)
			sw2.Mesh.Scale = sw2.Mesh.Scale + Vector3.new(1, 1, 1)
			sw.RotVelocity = sw.RotVelocity * i
			sw2.RotVelocity = sw.RotVelocity * i
			wait(0.25)			
		end			
		
		myTorso.Anchored = false
		if sw then sw:Remove() end
		if sw2 then sw2:Remove() end
	end
end

-- on touch the trident does damage
function blow(hit)
	-- rarely the parent of the object that touches the trident maybe nil, so have this check
	if hit.Parent then
		humanoid = hit.Parent:FindFirstChild("Humanoid")
	end
	vCharacter = Tool.Parent
	myHumanoid = vCharacter:FindFirstChild("Humanoid")
	-- Get a handle to the player object for leaderboard script
	vPlayer = game.Players:GetPlayerFromCharacter(vCharacter)
	if humanoid ~= nil and myHumanoid ~= nil and humanoid ~= myHumanoid then 
		rightArm = vCharacter:FindFirstChild("Right Arm")
		if rightArm then 
			rightGrip = rightArm:FindFirstChild("RightGrip")
			-- check if trident is welded with the right arm
			if rightGrip and (rightGrip.Part0 == Trident or rightGrip.Part1 == Trident) then
				if not GLib.IsTeammate(GLib.GetPlayerFromPart(humanoid), GLib.GetPlayerFromPart(vCharacter)) then
					tagHumanoid(humanoid, vPlayer)
					humanoid:TakeDamage(damage)
				end
			end
		end   
	end
end

-- tag the humanoid for leaderboard stuff
function tagHumanoid(humanoid, player)
	if humanoid ~= nil then 
		creator = Instance.new("ObjectValue")
		creator.Name = "creator"
		creator.Value = player
		creator.Parent = humanoid
		-- Mark the tag to be collected after 1 second
		debris:addItem(creator, 1)
	end
end

function onActivated()
	-- if the Tool is not enabled then return
	if Tool.Enabled == false then 
		return
	end

	Tool.Enabled = false

	vCharacter = Tool.Parent
	myTorso = vCharacter:FindFirstChild("Torso")
	if myTorso == nil then 
		return
	end

	wait(1.5)
	Tool.Enabled = true 
end

function onEquipped()
	vCharacter = Tool.Parent
	myTorso = vCharacter:FindFirstChild("Torso")
end

function onUnequipped()
	-- Stop the sounds
	if quakeSound then quakeSound:Stop() end
end

Tool:WaitForChild'ActivateQuake'.OnServerEvent:connect(function(c,a) quake(a) end)

Tool.Activated:connect(onActivated)
Tool.Equipped:connect(onEquipped)
Tool.Unequipped:connect(onUnequipped)

Trident.Touched:connect(blow)
]]></ProtectedString>
