				<ProtectedString name="Source"><![CDATA[local GLib = require(206209239)
local bodyVelocity = Instance.new("BodyVelocity")
bodyVelocity.P = 10000
bodyVelocity.Name = "QuakeVel"
bodyVelocity.maxForce = Vector3.new(10000, 0, 10000)

script.Parent.QuakeRemote.OnServerEvent:connect(function(client, character)
	if client.Character == script.Parent.Parent then
		if not GLib.IsTeammate(client, GLib.GetPlayerFromPart(character)) then
			local quakeScript = character:FindFirstChild'QuakeScript'
			if quakeScript == nil then
				quakeScript = script.QuakeScript:Clone()
				quakeScript.Parent = character
				quakeScript.Disabled = false
			end
		end
	end
end)

script.Parent.QuakePartRemote.OnServerEvent:connect(function(client, parts, myTorso)
	if client.Character == script.Parent.Parent then
		for i = 1, #parts do 
			if parts[i].Anchored == false then 
			newBV = bodyVelocity:Clone()
			distance = (myTorso.Position - parts[i].Position).magnitude
			newBV.velocity = (parts[i].Position - myTorso.Position).unit * 40  
			newBV.Parent = parts[i]					
			tempHumanoid = parts[i].Parent:FindFirstChild("Humanoid")
			if tempHumanoid then if distance > 10 then tempHumanoid:TakeDamage(8) else tempHumanoid:TakeDamage(12) end end
			end
		end	
	end
	wait(2.0)
	for i = 1, #parts do 
		bv = parts[i]:FindFirstChild("QuakeVel")
		if bv then bv:Remove() end
	end
end)]]></ProtectedString>
