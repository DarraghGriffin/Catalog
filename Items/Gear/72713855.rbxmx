local storeWelds = {}

function gripCheck(newGrip)
&#9;if newGrip.className == &quot;Weld&quot; and newGrip.Name == &quot;RightGrip&quot; then
&#9;&#9;newGrip.C0 = newGrip.C0 + (scaleFactor - 1) * newGrip.C0.p
&#9;&#9;newGrip.C1 = newGrip.C1 + (scaleFactor - 1) * newGrip.C1.p
&#9;end
end

function toolGrowCheck(newTool)
&#9;if newTool.className == &quot;Tool&quot; and newTool:FindFirstChild(&quot;Handle&quot;) and newTool.Handle:FindFirstChild(&quot;Mesh&quot;) then
&#9;&#9;newTool.Handle.Mesh.Scale = newTool.Handle.Mesh.Scale * scaleFactor
&#9;&#9;newTool.GripPos = newTool.GripPos * 1.5
&#9;end
end

function toolShrinkCheck(newTool)
&#9;if newTool.className == &quot;Tool&quot; and newTool:FindFirstChild(&quot;Handle&quot;) and newTool.Handle:FindFirstChild(&quot;Mesh&quot;) then
&#9;&#9;newTool.Handle.Mesh.Scale = newTool.Handle.Mesh.Scale / scaleFactor
&#9;&#9;newTool.GripPos = newTool.GripPos/1.5
&#9;end
end

function onEquipped()
&#9;vCharacter = Tool.Parent 
&#9;myHumanoid = vCharacter:FindFirstChild(&quot;Humanoid&quot;)
&#9;myTorso = vCharacter:FindFirstChild(&quot;Torso&quot;)
&#9;vPlayer = game.Players:GetPlayerFromCharacter(vCharacter)
&#9;myRightArm = vCharacter:FindFirstChild(&quot;Right Arm&quot;)
&#9;storeCharacter = vCharacter 
&#9;local torsoChildren = myTorso:GetChildren()
&#9;for i = 1, #torsoChildren do 
&#9;&#9;if torsoChildren[i]:IsA(&quot;Motor6D&quot;) then 
&#9;&#9;&#9;table.insert(storeWelds, torsoChildren[i])
&#9;&#9;end 
&#9;end 
&#9;waitForChild(vPlayer, &quot;PlayerGui&quot;)
&#9;waitForChild(vPlayer.PlayerGui, &quot;HealthGUI&quot;) 
&#9;healthGui = vPlayer.PlayerGui.HealthGUI:Clone() 
&#9;print(healthGui)
&#9;waitForChild(vCharacter, &quot;HealthScript v3.1&quot;)
&#9;healthScript = vCharacter[&quot;HealthScript v3.1&quot;]
&#9;
end 

function onUnequipped()
end

Tool.Equipped:connect(onEquipped)
Tool.Unequipped:connect(onUnequipped)

while Tool:FindFirstChild(&quot;MonstaTime&quot;) == nil do 
&#9;wait()
end 

monstaTime = Tool.MonstaTime

monstaTime.Changed:connect(function()
&#9;if monstaTime and monstaTime.Value == true and vPlayer then 
&#9;&#9;print(1)
&#9;&#9;items = {}
&#9;&#9;local myChar = hulk:Clone() 
&#9;&#9;myChar.Name = vCharacter.Name 
&#9;&#9;local initPos = myTorso.CFrame
&#9;&#9;myChar:MoveTo(myTorso.Position)&#9;
&#9;&#9;backpack = vPlayer:FindFirstChild(&quot;Backpack&quot;)
&#9;&#9;if backpack then 
&#9;&#9;&#9;local backpackChildren = backpack:GetChildren()
&#9;&#9;&#9;for i = 1, #backpackChildren do 
&#9;&#9;&#9;&#9;if backpackChildren[i]:IsA(&quot;Tool&quot;) then
&#9;&#9;&#9;&#9;&#9;table.insert(items, backpackChildren[i])
&#9;&#9;&#9;&#9;end 
&#9;&#9;&#9;end 
&#9;&#9;end 
&#9;&#9;vPlayer.Character = myChar 
&#9;&#9;vCharacter = myChar 
&#9;&#9;vCharacter.Parent = game.Workspace&#9;&#9;
&#9;&#9;myHumanoid = vCharacter:FindFirstChild(&quot;Humanoid&quot;)
&#9;&#9;myTorso = vCharacter:FindFirstChild(&quot;Torso&quot;)&#9;&#9;
&#9;&#9;myRightArm = vCharacter:FindFirstChild(&quot;Right Arm&quot;)

&#9;&#9;if myHumanoid then 
&#9;&#9;&#9;local ratio = myHumanoid.Health/myHumanoid.MaxHealth
&#9;&#9;&#9;myHumanoid.WalkSpeed = 24.0
&#9;&#9;&#9;while myHumanoid.MaxHealth ~= 500 do 
&#9;&#9;&#9;&#9;myHumanoid.MaxHealth = 500 
&#9;&#9;&#9;end&#9;&#9;&#9;

&#9;&#9;&#9;while myHumanoid.Health ~= ratio * myHumanoid.MaxHealth do 
&#9;&#9;&#9;&#9;myHumanoid.Health = ratio * myHumanoid.MaxHealth
&#9;&#9;&#9;end
&#9;&#9;end 

&#9;&#9;if healthScript then  
