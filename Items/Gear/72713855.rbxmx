				<ProtectedString name="Source">local Tool = script.Parent 
local Potion = Tool.Handle

Tool.Enabled = true 

local vCharacter 
local myTorso 
local myHumanoid 

local drinkAnim
local rageAnim 

local rageSound 

function onActivated()
&#9;if Tool.Enabled == false then 
&#9;&#9;return 
&#9;end 

&#9;if myTorso == nil or myHumanoid == nil then 
&#9;&#9;return 
&#9;end 

&#9;Tool.Enabled = false

&#9;drinkAnim = myHumanoid:LoadAnimation(Tool.Drink)
&#9;if drinkAnim then drinkAnim:Play() end

&#9;wait(1.25)

&#9;rageAnim = myHumanoid:LoadAnimation(Tool.RageAnim)
&#9;if rageAnim then rageAnim:Play() end

&#9;wait(0.6)

&#9;local monstaTime = Tool:FindFirstChild(&quot;MonstaTime&quot;)
&#9;if monstaTime and monstaTime.Value == false then print(0.1) monstaTime.Value = true else print(&quot;Wat&quot;) print(monstaTime)end 

&#9;rageSound = Potion:FindFirstChild(&quot;RageSound&quot;)
&#9;if rageSound then rageSound:Play() end

&#9;wait(0.5) 
&#9;if rageAnim then rageAnim:Play() end

&#9;wait(5.0)
&#9;Tool.Enabled = true 
end 

function onEquipped()
&#9;vCharacter = Tool.Parent 
&#9;myTorso = vCharacter:FindFirstChild(&quot;Torso&quot;)
&#9;myHumanoid = vCharacter:FindFirstChild(&quot;Humanoid&quot;)
end

function onUnequipped()
&#9;if drinkAnim then drinkAnim:Stop() end 
&#9;if rageAnim then rageAnim:Stop() end
&#9;if rageSound then rageSound:Stop() end
end

Tool.Activated:connect(onActivated)
Tool.Equipped:connect(onEquipped)
Tool.Unequipped:connect(onUnequipped)

