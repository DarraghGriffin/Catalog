&#9;&#9;&#9;&#9;Potion.CanCollide = true 
&#9;&#9;&#9;&#9;Tool.Parent = vCharacter 
&#9;&#9;&#9;&#9;if toolGripConn then toolGripConn:disconnect() end 
&#9;&#9;&#9;&#9;if toolGrowConn then toolGrowConn:disconnect() end 
&#9;&#9;&#9;&#9;if toolShrinkConn then toolShrinkConn:disconnect() end 
&#9;&#9;&#9;&#9;vPlayer = game.Players:GetPlayerFromCharacter(vCharacter)
&#9;&#9;&#9;&#9;if healthScript then  
&#9;&#9;&#9;&#9;&#9;healthScript.Disabled = true 
&#9;&#9;&#9;&#9;&#9;healthScript.Parent = vCharacter&#9;&#9;&#9;
&#9;&#9;&#9;&#9;&#9;waitForChild(vPlayer,  &quot;PlayerGui&quot;)
&#9;&#9;&#9;&#9;&#9;if healthGui then 
&#9;&#9;&#9;&#9;&#9;&#9;healthGui:Clone().Parent = healthScript 
&#9;&#9;&#9;&#9;&#9;else 
&#9;&#9;&#9;&#9;&#9;&#9;print(&quot;HealthGui not found&quot;) 
&#9;&#9;&#9;&#9;&#9;end
&#9;&#9;&#9;&#9;&#9;healthScript.Disabled = false 
&#9;&#9;&#9;&#9;end 
&#9;&#9;&#9;&#9;waitForChild(vPlayer, &quot;Backpack&quot;)
