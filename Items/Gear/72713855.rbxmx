&#9;&#9;&#9;&#9;&#9;for i = 1, #backpackChildren do 
&#9;&#9;&#9;&#9;&#9;&#9;if backpackChildren[i]:IsA(&quot;Tool&quot;) then
&#9;&#9;&#9;&#9;&#9;&#9;&#9;table.insert(items, backpackChildren[i])
&#9;&#9;&#9;&#9;&#9;&#9;end 
&#9;&#9;&#9;&#9;&#9;end 
&#9;&#9;&#9;&#9;end 

&#9;&#9;&#9;&#9;local charChildren = vCharacter:GetChildren()
&#9;&#9;&#9;&#9;for i = 1, #charChildren do 
&#9;&#9;&#9;&#9;&#9;if charChildren[i]:IsA(&quot;Tool&quot;) then 
&#9;&#9;&#9;&#9;&#9;&#9;table.insert(items, charChildren[i])
&#9;&#9;&#9;&#9;&#9;end 
&#9;&#9;&#9;&#9;end 
&#9;&#9;&#9;&#9;vPlayer.Character = storeCharacter
&#9;&#9;&#9;&#9;vCharacter = storeCharacter 
&#9;&#9;&#9;&#9;vCharacter.Parent = game.Workspace &#9;
&#9;&#9;&#9;&#9;myHumanoid = vCharacter:FindFirstChild(&quot;Humanoid&quot;)
&#9;&#9;&#9;&#9;myTorso = vCharacter:FindFirstChild(&quot;Torso&quot;)&#9;&#9;
&#9;&#9;&#9;&#9;myRightArm = vCharacter:FindFirstChild(&quot;Right Arm&quot;)
&#9;&#9;&#9;&#9;if myTorso then 
&#9;&#9;&#9;&#9;&#9;for i = 1, #storeWelds do 
&#9;&#9;&#9;&#9;&#9;&#9;storeWelds[i].Parent = myTorso 
&#9;&#9;&#9;&#9;&#9;end 
&#9;&#9;&#9;&#9;&#9;myTorso.CFrame = teleportCFrame 
