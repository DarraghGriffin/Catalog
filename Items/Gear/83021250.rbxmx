				<ProtectedString name="Source">--Rescripted by Luckymaxer

Tool = script.Parent
Handle = Tool:WaitForChild(&quot;Handle&quot;)
Fire = Handle:WaitForChild(&quot;Fire&quot;)

Players = game:GetService(&quot;Players&quot;)
Debris = game:GetService(&quot;Debris&quot;)

BlindScript = script:WaitForChild(&quot;BlindScript&quot;)

BasePart = Instance.new(&quot;Part&quot;)
BasePart.Shape = Enum.PartType.Block
BasePart.Material = Enum.Material.Plastic
BasePart.TopSurface = Enum.SurfaceType.Smooth
BasePart.BottomSurface = Enum.SurfaceType.Smooth
BasePart.FormFactor = Enum.FormFactor.Custom
BasePart.Anchored = false
BasePart.Locked = true
BasePart.CanCollide = true

EyePart = BasePart:Clone()
EyePart.Name = &quot;EyePart&quot;
EyePart.Size = Vector3.new(1, 2, 1)
EyeMesh = Instance.new(&quot;SpecialMesh&quot;)
EyeMesh.MeshType = Enum.MeshType.FileMesh
EyeMesh.MeshId = &quot;http://www.roblox.com/asset/?id=69433615&quot;
EyeMesh.TextureId = &quot;http://www.roblox.com/asset/?id=69433631&quot;
EyeMesh.Scale = Vector3.new(1.1, 1.1, 1.1) 
EyeMesh.Parent = EyePart

HatPart = EyePart:Clone()
HatPart.Name = &quot;HatPart&quot;
HatPart.Size = Vector3.new(2, 2, 2)
HatMesh = Instance.new(&quot;SpecialMesh&quot;)
HatMesh.MeshType = Enum.MeshType.FileMesh
HatMesh.MeshId = &quot;http://www.roblox.com/asset/?id=22009651&quot;
HatMesh.TextureId = &quot;http://www.roblox.com/asset/?id=22009647&quot;
HatMesh.Scale = Vector3.new(1, 1, 1)
HatMesh.Parent = HatPart

Fire.Enabled = false
Tool.Enabled = true

function RayCast(Position, Direction, MaxDistance, IgnoreList)
&#9;return game:GetService(&quot;Workspace&quot;):FindPartOnRayWithIgnoreList(Ray.new(Position, Direction.unit * (MaxDistance or 999.999)), IgnoreList) 
end

function FireBeam(StartPos, EndPos)
&#9;local Distance = (StartPos - EndPos).magnitude
&#9;Distance = ((Distance &gt; 500 and 500) or Distance)
&#9;local Laser = BasePart:Clone()
&#9;Laser.Name = &quot;Bullet&quot;
&#9;Laser.Anchored = true
&#9;Laser.CanCollide = false
&#9;Laser.Size = Vector3.new(0.2, 0.2, (Distance + 0.2))
&#9;Laser.BrickColor = BrickColor.new(&quot;Bright red&quot;)
&#9;Laser.CFrame = CFrame.new(StartPos, EndPos) * CFrame.new(0, 0, -(Distance / 2))
&#9;Laser.Parent = game.Workspace&#9;
&#9;Debris:AddItem(Laser, 0.25)
end 

function LaunchSun()
&#9;
&#9;local origcharacter = Character

&#9;local Sun = Handle:Clone()
&#9;local SunMesh = Sun.Mesh
&#9;
&#9;Handle.Transparency = 1
&#9;
&#9;Sun.Name = &quot;Sun&quot;
&#9;Sun.Transparency = 0
&#9;Sun.Anchored = true
&#9;Sun.CFrame = Handle.CFrame
&#9;Debris:AddItem(Sun, 10) 
&#9;Sun.Parent = game:GetService(&quot;Workspace&quot;)

&#9;Humanoid.WalkSpeed = 2

&#9;local Fire = Instance.new(&quot;Fire&quot;)
&#9;Fire.Size = 30
&#9;Fire.Heat = 25
&#9;Fire.Parent = Sun
&#9;
&#9;
&#9;local Eye = EyePart:Clone()
&#9;local EyeWeld = Instance.new(&quot;Weld&quot;)
&#9;EyeWeld.Part0 = Head
&#9;EyeWeld.Part1 = Eye
&#9;EyeWeld.C1 = CFrame.new(0, -0.0500000305, -0.25, 1, 0, 0, 0, -4.37113883e-008, -1, 0, 1, -4.37113883e-008)
&#9;EyeWeld.Parent = Eye
&#9;Debris:AddItem(Eye, 7.0)
&#9;EyePart.Parent = Character

&#9;local Hats = {}
&#9;
&#9;for i, v in pairs(Character:GetChildren()) do
&#9;&#9;if v:IsA(&quot;Hat&quot;) then
&#9;&#9;&#9;table.insert(Hats, v)
&#9;&#9;&#9;v.Parent = nil
&#9;&#9;end
&#9;end
&#9;
&#9;local Hat = HatPart:Clone()
&#9;local HatWeld = Instance.new(&quot;Weld&quot;)
&#9;HatWeld.Part0 = Head
&#9;HatWeld.Part1 = Hat
&#9;HatWeld.Parent = Head 
&#9;HatWeld.C1 = CFrame.new(0, -0.799999952, 0, 1, 0, 0, 0, 1, 0, 0, 0, 1)
&#9;Debris:AddItem(Hat, 6.5)
&#9;Hat.Parent = Character
&#9;
&#9;local Duration = 2.75
&#9;local DurationTick = tick()
&#9;while (tick() - DurationTick) &lt; Duration do
&#9;&#9;wait()
&#9;&#9;Sun.CFrame = Sun.CFrame + Vector3.new(0, 2, 0) 
&#9;&#9;SunMesh.Scale = SunMesh.Scale * 1.05 
&#9;end &#9;&#9;

&#9;for i, v in pairs(Players:GetChildren()) do
&#9;&#9;if v:IsA(&quot;Player&quot;) and v ~= Player and v.Character then
&#9;&#9;&#9;local character = v.Character
&#9;&#9;&#9;local humanoid = character:FindFirstChild(&quot;Humanoid&quot;)
&#9;&#9;&#9;local torso = character:FindFirstChild(&quot;Torso&quot;)
&#9;&#9;&#9;local Blind = false
&#9;&#9;&#9;if humanoid and humanoid.Health &gt; 0 and torso then
&#9;&#9;&#9;&#9;local Distance = (Sun.Position - torso.Position).magnitude
&#9;&#9;&#9;&#9;if Distance &lt;= 240 then
&#9;&#9;&#9;&#9;&#9;Blind = true
&#9;&#9;&#9;&#9;&#9;FireBeam(Sun.Position, torso.Position)
&#9;&#9;&#9;&#9;else
&#9;&#9;&#9;&#9;&#9;local RayPos, RayHit = RayCast(Sun.Position, ((torso.Position - Sun.Position).unit * 500), MaxDistance, {Character})
&#9;&#9;&#9;&#9;&#9;if RayHit then
&#9;&#9;&#9;&#9;&#9;&#9;if RayHit.Parent:IsA(&quot;Hat&quot;) or RayHit.Parent:IsA(&quot;Tool&quot;) then
&#9;&#9;&#9;&#9;&#9;&#9;&#9;if RayHit.Parent == character then
&#9;&#9;&#9;&#9;&#9;&#9;&#9;&#9;FireBeam(Sun.Position, torso.Position)
&#9;&#9;&#9;&#9;&#9;&#9;&#9;&#9;Blind = true
&#9;&#9;&#9;&#9;&#9;&#9;&#9;end
&#9;&#9;&#9;&#9;&#9;&#9;end
&#9;&#9;&#9;&#9;&#9;end
&#9;&#9;&#9;&#9;end
&#9;&#9;&#9;&#9;if Blind and not character:FindFirstChild(BlindScript.Name) then 
&#9;&#9;&#9;&#9;&#9;local BlindScriptClone = BlindScript:Clone()
&#9;&#9;&#9;&#9;&#9;BlindScriptClone.Disabled = false
&#9;&#9;&#9;&#9;&#9;local Creator = Instance.new(&quot;ObjectValue&quot;)
&#9;&#9;&#9;&#9;&#9;Creator.Name = &quot;Creator&quot;
&#9;&#9;&#9;&#9;&#9;Creator.Value = Player
&#9;&#9;&#9;&#9;&#9;Creator.Parent = BlindScriptClone
&#9;&#9;&#9;&#9;&#9;Debris:AddItem(BlindScriptClone, 12)
&#9;&#9;&#9;&#9;&#9;BlindScriptClone.Parent = character
&#9;&#9;&#9;&#9;end  
&#9;&#9;&#9;end
&#9;&#9;end
&#9;end&#9;

&#9;wait(0.75)
&#9;
&#9;Humanoid.WalkSpeed = 16
&#9;
&#9;wait(6.25)
&#9;
&#9;if origcharacter then
&#9;&#9;for i, v in pairs(Hats) do
&#9;&#9;&#9;v.Parent = origcharacter
&#9;&#9;end
&#9;end
&#9;
&#9;Handle.Transparency = 0
&#9;
end 

function Activated()
&#9;if not Tool.Enabled or not ToolEquipped or not CheckIfAlive() or (Sun and Sun.Parent) then
&#9;&#9;return
&#9;end
&#9;
&#9;Tool.Enabled = false 

&#9;LaunchSun()
&#9;&#9;
&#9;Tool.Enabled = true  
end 

function CheckIfAlive()
&#9;return (((Player and Player.Parent and Humanoid and Humanoid.Parent and Humanoid.Health &gt; 0 and Head and Head.Parent and Torso and Torso.Parent) and true) or false)
end

function Equipped()
&#9;Character = Tool.Parent
&#9;Player = Players:GetPlayerFromCharacter(Character)
&#9;Humanoid = Character:FindFirstChild(&quot;Humanoid&quot;)
&#9;Head = Character:FindFirstChild(&quot;Head&quot;)
&#9;Torso = Character:FindFirstChild(&quot;Torso&quot;)
&#9;if not CheckIfAlive() then
&#9;&#9;return
&#9;end
&#9;ToolEquipped = true
end

function Unequipped()
&#9;ToolEquipped = false
end

Tool.Activated:connect(Activated)

Tool.Equipped:connect(Equipped)
Tool.Unequipped:connect(Unequipped)</ProtectedString>
