function DarkEffect()

	local TempSounds = {}
	
	if Head then
		for i, v in pairs(Sounds) do
			if v then
				local Sound = v:Clone()
				Debris:AddItem(Sound, 4)
				Sound.Parent = Head
				TempSounds[v.Name] = Sound
			end
		end
	end
	
	if TempSounds.IceForm then
		TempSounds.IceForm:Play()
	end
	
	local EffectedInstances = GetAllInstances(Character)
