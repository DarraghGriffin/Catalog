				<string name="Name">tigerScript</string>
				<ProtectedString name="Source">local Tool = script.Parent;

enabled = true

local hitEnabled= true;


function applyRandForce(limb)
&#9;local tempF= Instance.new(&quot;BodyForce&quot;);
&#9;tempF.force= Vector3.new((math.random()*500)-250,math.random()*500,(math.random()*500)-250);
&#9;tempF.Parent=limb;

&#9;wait(1);
&#9;tempF:Destroy();

end

function tearApart(hit)
&#9;local enemy=game.Players:GetPlayerFromCharacter(hit.Parent);
&#9;if not hitEnabled or not enemy or hit.Parent==Tool.Parent then
&#9;&#9;return;
&#9;end
&#9;hitEnabled=false;
&#9;local limbs={hit.Parent:FindFirstChild(&quot;Left Arm&quot;),hit.Parent:FindFirstChild(&quot;Right Arm&quot;),
&#9;&#9;hit.Parent:FindFirstChild(&quot;Left Leg&quot;),hit.Parent:FindFirstChild(&quot;Right Leg&quot;)};
&#9;local rand=math.random(1,4);
&#9;
&#9;if limbs[rand] then
&#9;&#9;limbs[rand].CanCollide=true;
&#9;&#9;limbs[rand]:BreakJoints();
&#9;&#9;Tool.Handle.roarSound:Play();
&#9;&#9;applyRandForce(limbs[rand]);
&#9;end
&#9;wait(2)
&#9;hitEnabled=true;

end



function onActivated()
&#9;if not enabled  then
&#9;&#9;return
&#9;end
&#9;enabled = false

&#9;Tool.Handle.Transparency=1;
&#9;Tool.Handle.roarSound:Play()

&#9;wait(3)
&#9;local oldSpeed=Tool.Parent.Humanoid.WalkSpeed;
&#9;Tool.Parent.Humanoid.WalkSpeed=Tool.Parent.Humanoid.WalkSpeed*1.5;
&#9;Tool.playAni.Value=not Tool.playAni.Value
&#9;local hitfunc=Tool.Parent.Torso.Touched:connect(tearApart);
&#9;

&#9;local tHat=Tool.LionHat:Clone();
&#9;tHat.Handle.Fire.Enabled=true;
&#9;tHat.Parent=Tool.Parent;
&#9;tHat.Handle.Anchored=false;
&#9;tHat.Handle.Transparency=0;
&#9;wait(10);
&#9;hitfunc:disconnect();
&#9;tHat:Destroy();
&#9;Tool.endAni.Value=not Tool.endAni.Value
&#9;wait(5);
&#9;Tool.Parent.Humanoid.WalkSpeed=oldSpeed;
&#9;Tool.Handle.Transparency=0;
&#9;enabled = true;

end



script.Parent.Activated:connect(onActivated)

function OnUnequip()
&#9;--Tool.endAni:Fire();
end
script.Parent.Unequipped:connect(OnUnequip)
