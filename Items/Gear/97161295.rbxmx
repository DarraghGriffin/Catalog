				<ProtectedString name="Source"><![CDATA[--Made by Stickmasterluke


sp=script.Parent

rate=1/30
debris=game:GetService("Debris")
canclone=true

function waitfor(parent,name)
	while parent:FindFirstChild(name)==nil do
		wait()
	end
	return parent:FindFirstChild(name)
end

moveint=22
scrollspeed=.1

function creategui()
	tool=nil
	local plr = game.Players:getPlayerFromCharacter(sp.Parent)
	if plr:FindFirstChild("PlayerGui")~=nil and plr:FindFirstChild("Backpack")~=nil then
		if gui~=nil then
			gui:remove()
		end
		gui=Instance.new("ScreenGui")
		gui.Name="ToolCloneGui"
		gui.Parent=plr.PlayerGui
		
		local bus = Instance.new'RemoteEvent'
		bus.Name = 'GuiBus'
		bus.Parent = script
		local ls = script.Parent.GuiScript:Clone()
		ls.Parent = gui
		ls.Bus.Value = bus
		ls.Tool.Value = sp
		ls.Disabled = false

		showframe=Instance.new("Frame")
		showframe.Name = 'ShowFrame'
		showframe.Style="RobloxRound"
		showframe.Position = UDim2.new(1,-480,1,-410)
		showframe.Size = UDim2.new(0,160,0,200)
		showframe.ClipsDescendants=true
		showframe.Visible=false
		showframe.Parent = gui

		showimage=Instance.new("ImageLabel")
		showimage.Name = 'ShowImage'
		showimage.Size=UDim2.new(1,0,0,150)
		showimage.BackgroundTransparency=1
		showimage.Parent=showframe

		closebutton=Instance.new("TextButton")
		closebutton.Name = 'CloseButton'
		closebutton.Text="X"
		closebutton.TextScaled=true
		closebutton.Style="RobloxButtonDefault"
		closebutton.Size=UDim2.new(0,30,0,30)
		closebutton.TextColor3=Color3.new(1,0,0)
		closebutton.Font="Arial"
		closebutton.FontSize="Size14"
		closebutton.Position=UDim2.new(1,-20,0,-10)
		closebutton.Parent=showframe

		clonebutton=Instance.new("TextButton")
		clonebutton.Name = 'CloneButton'
		clonebutton.Text="Clone"
		clonebutton.Style="RobloxButton"
		clonebutton.Size=UDim2.new(0,80,0,30)
		clonebutton.TextColor3=Color3.new(1,1,1)
		clonebutton.Font="ArialBold"
		clonebutton.FontSize="Size14"
		clonebutton.Position=UDim2.new(.5,-40,1,-35)
		clonebutton.Parent=showframe
		

		frame=Instance.new("Frame")
		frame.Name = 'Frame'
		frame.Style="RobloxRound"
		frame.Position = UDim2.new(1,-300,1,-410)
		frame.Size = UDim2.new(0,200,0,323)
		frame.ClipsDescendants=true
		frame.Parent = gui

		window=Instance.new("Frame")
		window.Name = 'Window'
		window.BackgroundTransparency=1
		window.ClipsDescendants=true
		window.Parent=frame

		scroller=Instance.new("Frame")
		scroller.Name = 'Scroller'
		scroller.BackgroundTransparency=.5
		scroller.BorderSizePixel=0
		scroller.Position = UDim2.new(1,-25,0,0)
		scroller.Size = UDim2.new(0,25,1,0)
		scroller.Parent = frame

		local buttonup=Instance.new("ImageButton")
		buttonup.Name = 'ButtonUp'
		buttonup.Size=UDim2.new(0,25,0,25)
		buttonup.Position=UDim2.new(0,0,0,0)
		buttonup.Image="rbxasset://textures/ui/scrollbuttonUp.png"
		buttonup.BackgroundTransparency=1
		buttonup.Parent=scroller

		local buttondown=Instance.new("ImageButton")
		buttondown.Name = 'ButtonDown'
		buttondown.Size=UDim2.new(0,25,0,25)
		buttondown.Position=UDim2.new(0,0,1,-25)
		buttondown.BackgroundTransparency=1
		buttondown.Image="rbxasset://textures/ui/scrollbuttonDown.png"
		buttondown.Parent=scroller

		i=0
		for _,v in ipairs(plr.Backpack:GetChildren()) do
			if v.className=="Tool" then
				local button=Instance.new("TextButton")
				button.Size=UDim2.new(1,0,0,20)
				button.Position=UDim2.new(0,0,0,i*22)
				button.TextColor3=Color3.new(1,1,1)
				button.Style="RobloxButton"
				button.Text=v.Name
				button.Visible=true
				button.Parent=window
				local val = Instance.new'ObjectValue'
				val.Value = v
				val.Name = 'Tool'
				val.Parent = button
				i=i+1
			end
		end
		window.Size=UDim2.new(1,-30,0,(i+1)*22)
		
		bus.OnServerEvent:connect(function(client, action, ...)
			if client.Character == sp.Parent then
				if action == 'Clone' then
					local tool = ...
					
					local ctool=tool:clone()
					--ctool.CanBeDropped=true
					local t=sp.Parent:FindFirstChild("Torso")
					local hndl=ctool:FindFirstChild("Handle")
					if hndl~=nil and t~=nil and canclone then
						canclone=false
						gui:remove()
						hndl.CFrame=t.CFrame+(t.CFrame.lookVector*10)+Vector3.new(0,5,0)
						ctool.Parent=game.Workspace
						local s=Instance.new("Sound")
						s.Name="PhaseInSoundEffect"
						s.SoundId="http://www.roblox.com/asset/?id=97272128"
						debris:AddItem(s,5)
						s.Parent=hndl
						delay(.05,function()
							if s~=nil then
								s:Play()
							end
						end)
						delay(0,function()
							p=Instance.new("Part")
							p.BrickColor=BrickColor.new("Cyan")
							p.FormFactor="Custom"
							p.Size=Vector3.new(0,0,0)
							p.Anchored=true
							p.CanCollide=false
							p.TopSurface="Smooth"
							p.BottomSurface="Smooth"
							p.Name="Effect"
							local m=Instance.new("SpecialMesh")
							m.MeshType="Sphere"
							m.Parent=p
							debris:AddItem(p,5)
							p.CFrame=hndl.CFrame+Vector3.new(0,-3,0)
							p.Parent=game.Workspace
							frames=.5/rate
							for frame=1,frames do
								local percent=frame/frames
								m.Scale=Vector3.new(5,5,5)*(1-percent)*8
								p.Transparency=percent
								wait(rate)
							end
							sp:remove()
						end)
						wait(2)
						sp:remove()
					end
				end
			end
		end)
	end
end



function onEquipped()
	equipped=true
	creategui()
end

function onUnequipped()
	equipped=false
	if gui~=nil then
		gui:remove()
	end
end

sp.Equipped:connect(onEquipped)
sp.Unequipped:connect(onUnequipped)


]]></ProtectedString>
