		<Item class="LocalScript" referent="RBXA89B869F0DF6404AB825E23F533C7374">
			<Properties>
				<bool name="Disabled">true</bool>
				<Content name="LinkedSource"><null></null></Content>
				<string name="Name">GuiScript</string>
				<ProtectedString name="Source"><![CDATA[local gui = script.Parent
local plr = gui.Parent.Parent
local bus = script:WaitForChild'Bus'.Value or script.Bus.Changed:wait() -- hnnng
local tool = script:WaitForChild'Tool'.Value or script.Tool.Changed:wait()

local showframe = gui:WaitForChild'ShowFrame'
local showimage = showframe:WaitForChild'ShowImage'
local frame = gui:WaitForChild'Frame'
local scroller = frame:WaitForChild'Scroller'
local window = frame:WaitForChild'Window'

local scrollspeed = 0.1
local moveint = 22
local rate = 1/30

showframe:WaitForChild'CloseButton'.MouseButton1Down:connect(function()
	showframe.Visible=false
end)
showframe:WaitForChild'CloneButton'.MouseButton1Down:connect(function()
	local chr=plr.Character
	if tool~=nil and chr~=nil then
		bus:FireServer('Clone', tool)
	end
end)
scroller:WaitForChild'ButtonUp'.MouseButton1Down:connect(function()
	upbuttondown=true
	while upbuttondown do
		if window.Position.Y.Offset+moveint>=0 then
			window.Position=UDim2.new(0,0,0,0)
			upbuttondown=false
		else
			window.Position=window.Position+UDim2.new(0,0,0,moveint)
		end
		wait(scrollspeed)
	end
end)
scroller:WaitForChild'ButtonUp'.MouseButton1Up:connect(function()
	upbuttondown=false
end)
scroller:WaitForChild'ButtonUp'.MouseLeave:connect(function()
	upbuttondown=false
end)
scroller:WaitForChild'ButtonDown'.MouseButton1Down:connect(function()
	downbuttondown=true
	while downbuttondown do
		if (window.Position.Y.Offset+window.Size.Y.Offset)-moveint<=frame.Size.Y.Offset then
			window.Position=UDim2.new(0,0,0,frame.Size.Y.Offset-window.Size.Y.Offset)
			downbuttondown=false
		else
			window.Position=window.Position+UDim2.new(0,0,0,-moveint)
		end
		wait(scrollspeed)
	end
end)
scroller:WaitForChild'ButtonDown'.MouseButton1Up:connect(function()
	downbuttondown=false
end)
scroller:WaitForChild'ButtonDown'.MouseLeave:connect(function()
	downbuttondown=false
end)

local function ontoolgui(button)
	button.MouseButton1Down:connect(function()
		if button.Tool.Value~=nil then
			tool=button.Tool.Value
			showimage.Image=button.Tool.Value.TextureId
			showframe.Visible=true
		end
	end)
end
for index, obj in next, window:GetChildren() do ontoolgui(obj) end
window.ChildAdded:connect(ontoolgui)]]></ProtectedString>
